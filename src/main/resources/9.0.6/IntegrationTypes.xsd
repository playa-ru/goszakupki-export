					<xs:documentation>Подспособ определения поставщика</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="actual" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>Признак актуальности</xs:documentation>
				</xs:annotation>
