			<xs:enumeration value="EC"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="zfcs_purchasePlanPurchaseNumberType">
		<xs:annotation>
			<xs:documentation>Номер закупки, включенной в план закупок</xs:documentation>
