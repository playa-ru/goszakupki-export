														<xs:documentation>Место происхождения</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="price" type="base:moneyPositiveType" minOccurs="0">
													<xs:annotation>
														<xs:documentation>Цена за единицу</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="characteristics" minOccurs="0">
													<xs:annotation>
														<xs:documentation>Характеристики товара, работы услуги позиции КТРУ</xs:documentation>
													</xs:annotation>
													<xs:complexType>
														<xs:sequence>
															<xs:element name="characteristic" maxOccurs="unbounded">
																<xs:annotation>
																	<xs:documentation>Характеристика товара, работы услуги позиции КТРУ</xs:documentation>
																</xs:annotation>
																<xs:complexType>
																	<xs:sequence>
																		<xs:element name="characteristicName" type="base:ktruDictNameType">
																			<xs:annotation>
																				<xs:documentation>Наименование характеристики</xs:documentation>
																			</xs:annotation>
																		</xs:element>
																		<xs:element name="characteristicValues">
																			<xs:annotation>
																				<xs:documentation>Значения характеристики</xs:documentation>
																			</xs:annotation>
																			<xs:complexType>
																				<xs:sequence>
																					<xs:element name="characteristicValue" type="zfcs_KTRUCharacteristicValueType" maxOccurs="unbounded">
																						<xs:annotation>
																							<xs:documentation>Значение характеристики</xs:documentation>
																						</xs:annotation>
																					</xs:element>
																				</xs:sequence>
																			</xs:complexType>
																		</xs:element>
																	</xs:sequence>
																</xs:complexType>
															</xs:element>
														</xs:sequence>
													</xs:complexType>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="industryClassifier" type="zfcs_KTRUIndustryClassifierType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Позиция отраслевого классификатора, соответствующая позиции КТРУ. Устарело, не применяется</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="attachments" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Информация о прикрепленных документах</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="attachment" maxOccurs="unbounded">
										<xs:annotation>
											<xs:documentation>Прикрепленный документ</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:complexContent>
												<xs:extension base="zfcs_attachmentType">
													<xs:sequence minOccurs="0">
														<xs:element name="attachmentBusinessType" type="zfcs_KTRUAttachmentBusinessType" minOccurs="0">
															<xs:annotation>
																<xs:documentation>Бизнес-тип прикрепляемого документа:

D – документ; 
O – документ-основание; 
A – документ- основание аннулирования; 
S – документ-стандарт</xs:documentation>
															</xs:annotation>
														</xs:element>
													</xs:sequence>
												</xs:extension>
											</xs:complexContent>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="cancelInfo" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Информация об аннулировании позиции</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="cancelDate" type="xs:dateTime">
										<xs:annotation>
											<xs:documentation>Дата аннулирования</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="cancelReason" type="zfcs_longTextMinType">
										<xs:annotation>
											<xs:documentation>Причина аннулирования</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="nsiDescription" type="zfcs_longTextMinType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Описание</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="isTemplate" type="xs:boolean" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Позиция является укрупненной (шаблоном)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="parentPositionInfo" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Информация о родительской позиции КТРУ</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="code" type="base:ktruCodeType">
										<xs:annotation>
											<xs:documentation>Код  родительской позиции КТРУ</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="version" type="xs:int" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Номер версии родительской позиции КТРУ</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="externalCode" type="zfcs_KTRPositionExternalCodeType" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Внешний код позиции КТРУ </xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="externalCode" type="zfcs_KTRPositionExternalCodeType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Внешний код позиции КТРУ </xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="signData" type="zfcs_KTRUSignType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Данные подписи позиции. Не заполняется при выгрузке. Используется при формировании печатной формы</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!--КТРУ. Типы-->
	<xs:complexType name="zfcs_KTRUStandartType">
		<xs:annotation>
			<xs:documentation>Тип КТРУ: Информация о техническом регламенте или стандарте</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="docName" type="base:ktruDictNameType">
				<xs:annotation>
					<xs:documentation>Наименование стандарта (поле является ключом, идентифицирующим регламент или стандарт)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="docFileData" type="zfcs_attachmentType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Прикрепленный файл. Не заполняется при выгрузке. Используется при формировании печатной формы</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="zfcs_KTRUSignType">
		<xs:annotation>
			<xs:documentation>Тип КТРУ: Данные подписи</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="url">
				<xs:annotation>
					<xs:documentation>Ссылка для скачивания электронного документа</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
						<xs:maxLength value="1024"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="signature">
				<xs:annotation>
					<xs:documentation>Электронная подпись электронного документа</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:base64Binary">
							<xs:attribute name="type" type="base:signatureType">
								<xs:annotation>
									<xs:documentation>Тип электронной подписи:
CAdES-BES;
CAdES-A</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="zfcs_KTRUClassifierType">
		<xs:annotation>
			<xs:documentation>Тип КТРУ: Общероссийский или международный классификатор</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="name" type="base:ktruDictNameType">
				<xs:annotation>
					<xs:documentation>Наименование классификатора (поле является ключом, идентифицирующим классификатор)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="values">
				<xs:annotation>
					<xs:documentation>Значения классификатора, относящиеся к позиции КТРУ</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="value" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Значения классификатора, относящееся к позиции КТРУ</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="code" type="base:ktruClassifierCodeType" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Код значения классификатора (поле является ключом, идентифицирующим значение классификатора).
Поле обязательно для заполнения, необязательность в схеме для обратной совместимости</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="name" type="base:ktruDictNameType">
										<xs:annotation>
											<xs:documentation>Наименование значения классификатора</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="descriptionValue" type="zfcs_longTextMinType" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Неструктурированное описание значения классификатора </xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="zfcs_KTRUCharacteristicType">
		<xs:annotation>
			<xs:documentation>Тип КТРУ: Характеристика товаров, работ, услуг</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="code" type="base:ktruDictCodeType">
				<xs:annotation>
					<xs:documentation>Код характеристики</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="name" type="base:ktruDictNameType">
				<xs:annotation>
					<xs:documentation>Наименование характеристики (поле является ключом, идентифицирующим классификатор)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="type" type="base:ktruCharacteristicTypeType">
				<xs:annotation>
					<xs:documentation>Тип характеристики позиции КТРУ:

1 - качественная; 
2 - количественная.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="kind" type="base:ktruCharacteristicKindType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Вид характеристики позиции КТРУ:

1 - неизменяемая заказчиком; 
2 - изменемая заказчиком с выбором одного значения;
3 - изменяемая заказчиком, выбор нескольких значений.

Поле обязательно для заполнения, необязательность реализована для обратной совместимости</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="actual" type="xs:boolean" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Актуальность: 
true - актуально;
false - неактуально.

Поле обязательно для заполнения, необязательность реализована для обратной совместимости</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="isRequired" type="xs:boolean" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Характеристика обязательна к применению:

false - нет; 
true - да.

Поле обязательно для заполнения, необязательность реализована для обратной совместимости</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="choiceType" type="zfcs_KTRUCharacteristicChoiceTypeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Тип выбора значений характеристики:

1 – единственный выбор; 
2 – множественный выбор; 
3 – неизменяемый.

Используется в ведении КТРУ. Не используется в применении КТРУ.
Поле обязательно для заполнения, необязательность реализована для обратной совместимости</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="values">
				<xs:annotation>
					<xs:documentation>Допустимые значения характеристики позиции КТРУ</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="value" type="zfcs_KTRUCharacteristicValueType" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Допустимое значение характеристики позиции КТРУ</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="zfcs_KTRUIndustryClassifierType">
		<xs:annotation>
			<xs:documentation>Тип КТРУ: Отраслевой классификатор, соответствующий позиции КТРУ</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="classisfierName" type="zfcs_longTextMinType">
				<xs:annotation>
					<xs:documentation>Наименование отраслевого классификатора (поле является ключом, идентифицирующим классификатор)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="positionClassifierNode" type="zfcs_KTRUIndustryClassifierNodeType">
				<xs:annotation>
					<xs:documentation>Позиция отраслевого классификатора, соответствующая позиции КТРУ</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="zfcs_KTRUIndustryClassifierNodeType">
		<xs:annotation>
			<xs:documentation>Тип КТРУ: Позиция отраслевого классификатора, соответствующая позиции КТРУ</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="code" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Код позиции отраслевого классификатора. Поле обязательно для заполнения, необязательно по схеме для обратной совместимости</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
						<xs:maxLength value="20"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="name" type="zfcs_longTextMinType">
				<xs:annotation>
					<xs:documentation>Наименование позиции отраслевого классификатора (поле является ключом, идентифицирующим значение классификатора)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="parentNode" type="zfcs_KTRUIndustryClassifierNodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Родительская позиция отраслевого классификатора</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="zfcs_KTRUCharacteristicValueType">
		<xs:annotation>
			<xs:documentation>Тип КТРУ: Значение характеристики позиции КТРУ</xs:documentation>
		</xs:annotation>
		<xs:choice>
			<xs:sequence>
				<xs:annotation>
					<xs:documentation>Заполняется для качественной характеристики</xs:documentation>
				</xs:annotation>
				<xs:element name="qualityDescription" type="base:ktruDictNameType">
					<xs:annotation>
						<xs:documentation>Текстовое описание значения качественной характеристики</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
			<xs:sequence>
				<xs:annotation>
					<xs:documentation>Заполняется для количественной характеристики</xs:documentation>
				</xs:annotation>
				<xs:element name="OKEI" type="zfcs_OKEIRef" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Единица измерения. 
Ссылка на классификатор ОКЕИ (nsiOKEI). Допустимо указание как постоянных, так и временных единиц измерения (для которых установлено поле isTemporaryForKTRU в выгрузке справочника nsiOKEI).  Игнорируется при приеме для извещений предварительного отбора, начиная с версии 9.0</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="valueFormat" type="base:ktruCharacteristicValueFormatType" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Формат значения характеристики:

N-числовой;
A-дополнительный</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:choice>
					<xs:annotation>
						<xs:documentation>Заполняется для количественных характеристик</xs:documentation>
					</xs:annotation>
					<xs:element name="rangeSet">
						<xs:annotation>
							<xs:documentation>Набор диапазонов значений характеристик</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="valueRange">
									<xs:annotation>
										<xs:documentation>Диапазон значений</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:sequence>
											<xs:sequence minOccurs="0">
												<xs:annotation>
													<xs:documentation>В случае отсутствия должна быть задана верхняя граница диапазона (maxMathNotation + max) </xs:documentation>
												</xs:annotation>
												<xs:element name="minMathNotation" type="base:ktruMinMathNotationType" minOccurs="0">
													<xs:annotation>
														<xs:documentation>Математическое обозначение отношения к минимальному значению диапазона</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="min" minOccurs="0">
													<xs:annotation>
														<xs:documentation>Минимальное значение диапазона</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:decimal">
															<xs:totalDigits value="10"/>
															<xs:fractionDigits value="4"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
											</xs:sequence>
											<xs:sequence minOccurs="0">
												<xs:annotation>
													<xs:documentation>В случае отсутствия должна быть задана нижняя граница диапазона (minMathNotation + min)</xs:documentation>
												</xs:annotation>
												<xs:element name="maxMathNotation" type="base:ktruMaxMathNotationType" minOccurs="0">
													<xs:annotation>
														<xs:documentation>Математическое обозначение отношения к максимальному значению диапазона</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="max" minOccurs="0">
													<xs:annotation>
														<xs:documentation>Максимальное значение диапазона</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:decimal">
															<xs:totalDigits value="10"/>
															<xs:fractionDigits value="4"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
											</xs:sequence>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="valueSet">
						<xs:annotation>
							<xs:documentation>Набор конкретных значений характеристики</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="concreteValue">
									<xs:annotation>
										<xs:documentation>Конкретное значение</xs:documentation>
									</xs:annotation>
									<xs:simpleType>
