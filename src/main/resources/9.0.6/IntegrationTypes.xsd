						<xs:documentation>Информация о контракте размещена (для печатной формы)</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="regNum" type="oos:zfcs_contract.regNumType">
								<xs:annotation>
									<xs:documentation>Номер реестровой записи</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="number" type="zfcs_contract.Number2015Type" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Номер контракта (для печатной формы)</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="signDate" type="xs:date" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Дата заключения контракта (для печатной формы)</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="customer" type="zfcs_organizationInfoType" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Заказчик (для печатной формы)</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:sequence>
					<xs:element name="purchase" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Закупка (для печатной формы)</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="purchaseNumber" type="base:purchaseNumberType">
									<xs:annotation>
										<xs:documentation>Номер закупки</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="lotNumber" type="xs:integer" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Номер лота в извещении</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="purchaseObjectInfo" type="zfcs_longTextMinType" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Наименование объекта закупки (для печатной формы)</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="placingWay" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Подспособ определения поставщика (для печатной формы)</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:sequence>
											<xs:element name="code" type="base:placingWayCodeType">
												<xs:annotation>
													<xs:documentation>Код подспособа определения поставщика</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="name" minOccurs="0">
												<xs:annotation>
													<xs:documentation>Наименование подспособа определения поставщика</xs:documentation>
												</xs:annotation>
												<xs:simpleType>
													<xs:restriction base="xs:string">
														<xs:maxLength value="500"/>
													</xs:restriction>
												</xs:simpleType>
											</xs:element>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="customer" type="zfcs_organizationInfoType" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Заказчик (для печатной формы)</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="contractor" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Поставщик (подрядчик, исполнитель) (для печатной формы)</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:choice>
								<xs:element name="organizationRF">
									<xs:annotation>
										<xs:documentation>Юридическое лицо РФ</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:sequence>
											<xs:element name="fullName" type="zfcs_longTextType">
												<xs:annotation>
													<xs:documentation>Полное наименование</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="firmName" type="zfcs_longTextMinType" minOccurs="0">
												<xs:annotation>
													<xs:documentation>Фирменное наименование поставщика (подрядчика, исполнителя)</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="inn" type="base:innOrganizationType">
												<xs:annotation>
													<xs:documentation>ИНН организации</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="kpp" type="base:kppType" minOccurs="0">
												<xs:annotation>
													<xs:documentation>КПП организации</xs:documentation>
												</xs:annotation>
											</xs:element>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="personRF">
									<xs:annotation>
										<xs:documentation>Физическое лицо РФ</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:sequence>
											<xs:element name="name" type="zfcs_contactPersonType">
												<xs:annotation>
													<xs:documentation>ФИО поставщика</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="inn" type="base:innIndividualType" minOccurs="0">
												<xs:annotation>
													<xs:documentation>ИНН поставщика</xs:documentation>
												</xs:annotation>
											</xs:element>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="organizationForeignState">
									<xs:annotation>
										<xs:documentation>Юридическое лицо иностранного государства</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:sequence>
											<xs:element name="fullName" type="zfcs_longTextType">
												<xs:annotation>
													<xs:documentation>Полное наименование</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="firmName" type="zfcs_longTextMinType" minOccurs="0">
												<xs:annotation>
													<xs:documentation>Фирменное наименование поставщика (подрядчика, исполнителя)</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="inn" type="zfcs_innAnalogType">
												<xs:annotation>
													<xs:documentation>ИНН или аналог ИНН</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="kpp" type="base:kppType" minOccurs="0">
												<xs:annotation>
													<xs:documentation>КПП организации</xs:documentation>
												</xs:annotation>
											</xs:element>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="personForeignState">
									<xs:annotation>
										<xs:documentation>Физическое лицо иностранного государства</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:sequence>
											<xs:element name="name" type="zfcs_contactPersonType">
												<xs:annotation>
													<xs:documentation>ФИО поставщика</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="inn" type="zfcs_innAnalogType">
												<xs:annotation>
													<xs:documentation>ИНН или аналог ИНН</xs:documentation>
												</xs:annotation>
											</xs:element>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
							</xs:choice>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:choice>
			<xs:element name="attachments" type="zfcs_attachmentListType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Документы</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="extPrintForm" type="zfcs_extPrintFormType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Электронный документ, полученный из внешней системы</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="schemeVersion" type="base:schemeVersionType" use="required">
			<xs:annotation>
				<xs:documentation>Версия схемы</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:simpleType name="zfcs_revisionNumType">
		<xs:annotation>
			<xs:documentation>Номер редакции в РДИ</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="3"/>
			<xs:pattern value="\d{1,3}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="zfcs_registryNumType">
		<xs:annotation>
			<xs:documentation>Реестровый номер в ДИЗК</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="\d{8,19}"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Результаты аудита (РРА)-->
	<xs:simpleType name="zfcs_auditResultTypeEnum">
		<xs:annotation>
			<xs:documentation>Вид обобщенной информации:
E - Результаты экспертно-аналитической деятельности;
C - Результаты контрольной деятельности;
I - Результаты информационной деятельности;
O - Результаты иной деятельности;
R - Результаты аудита  привлечения подрядных организаций к выполнению работ по капитальному ремонту многоквартирных домов.
</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="E"/>
			<xs:enumeration value="C"/>
			<xs:enumeration value="I"/>
			<xs:enumeration value="O"/>
			<xs:enumeration value="R"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="zfcs_auditResultType">
		<xs:annotation>
			<xs:documentation>Обобщенная информация о результатах деятельности органа аудита в сфере закупок</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="id" type="xs:long" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Идентификатор документа</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="externalId" type="base:externalIdType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Внешний идентификатор документа</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="versionNumber" type="zfcs_revisionNumType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Номер редакции</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="registryNum" type="base:purchaseNumberType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Реестровый номер</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="docPublishDate" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Дата публикации документа</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="firstPublishDate" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Дата публикации документа первой редакции</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="publishOrg" type="zfcs_organizationInfoType">
				<xs:annotation>
					<xs:documentation>Орган аудита в сфере закупок, размещающий обобщенную информацию</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="href" type="base:hrefType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Гиперссылка на опубликованные сведения</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="printForm" type="zfcs_contract.printFormType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Печатная форма</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="extPrintForm" type="zfcs_extPrintFormType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Электронный документ, полученный из внешней системы</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="type" type="zfcs_auditResultTypeEnum">
				<xs:annotation>
					<xs:documentation>Вид обобщенной информации:
E - Результаты экспертно-аналитической деятельности;
C - Результаты контрольной деятельности;
I - Результаты информационной деятельности;
O - Результаты иной деятельности;
R - Результаты аудита  привлечения подрядных организаций к выполнению работ по капитальному ремонту многоквартирных домов.
</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="name" type="zfcs_longTextMinType">
				<xs:annotation>
					<xs:documentation>Наименование обобщенной информации</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="document" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Реквизиты документа</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="type" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Вид документа</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="100"/>
									<xs:minLength value="1"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="date" type="xs:dateTime" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Дата</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="number" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Номер</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="100"/>
									<xs:minLength value="1"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="period" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Исследуемый период</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="start" type="xs:dateTime" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Дата начала</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="end" type="xs:dateTime" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Дата окончания</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="addInfo" type="zfcs_longTextMinType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Дополнительная информация</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="action" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Информация о мероприятии</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="auditOrg" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Орган аудита в сфере закупок, размещающий обобщенную информацию</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:choice>
									<xs:element name="registered" type="zfcs_organizationInfoWithOgrnType">
										<xs:annotation>
											<xs:documentation>Зарегистрированная организация</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="nonregistered">
										<xs:annotation>
											<xs:documentation>Незарегистрированная организация</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:sequence>
												<xs:element name="fullName" type="oos:zfcs_longTextType">
													<xs:annotation>
														<xs:documentation>Полное наименование</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="OGRN" type="base:ogrnCodeType">
													<xs:annotation>
														<xs:documentation>ОГРН организации</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="OKTMO" type="zfcs_OKTMORef">
													<xs:annotation>
														<xs:documentation>ОКТМО организации</xs:documentation>
													</xs:annotation>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
								</xs:choice>
							</xs:complexType>
						</xs:element>
						<xs:element name="subjects">
							<xs:annotation>
								<xs:documentation>Предметы мероприятия</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="subject" type="zfcs_auditActionSubjectsRef" maxOccurs="unbounded">
										<xs:annotation>
											<xs:documentation>Предмет мероприятия</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="objects">
							<xs:annotation>
								<xs:documentation>Объекты мероприятия</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="object" maxOccurs="unbounded">
										<xs:annotation>
											<xs:documentation>Объект мероприятия</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:sequence>
												<xs:choice>
													<xs:element name="registered" type="zfcs_organizationInfoWithOgrnType">
														<xs:annotation>
															<xs:documentation>Зарегистрированная организация</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="nonregistered">
														<xs:annotation>
															<xs:documentation>Незарегистрированная организация</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:element name="fullName" type="zfcs_longTextType">
																	<xs:annotation>
																		<xs:documentation>Полное наименование</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="OGRN" type="base:ogrnCodeType">
																	<xs:annotation>
																		<xs:documentation>ОГРН организации. Элемент не используется в импорте</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="oktmo" type="zfcs_OKTMORef"/>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
												</xs:choice>
												<xs:element name="violations" minOccurs="0">
													<xs:annotation>
														<xs:documentation>Выявленные нарушения, недостатки</xs:documentation>
													</xs:annotation>
													<xs:complexType>
														<xs:sequence>
															<xs:element name="violation" type="zfcs_longTextMinType" maxOccurs="unbounded">
																<xs:annotation>
																	<xs:documentation>Нарушение</xs:documentation>
																</xs:annotation>
															</xs:element>
														</xs:sequence>
													</xs:complexType>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="period" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Срок проведения мероприятия</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="start" type="xs:dateTime" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Дата начала</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="end" type="xs:dateTime" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Дата окончания</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="attachments" type="zfcs_attachmentListType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Документы</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="modificationReason" type="zfcs_longTextMinType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Причина редактирования информации</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="schemeVersion" type="base:schemeVersionType" use="required">
			<xs:annotation>
				<xs:documentation>Версия схемы</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<!--Реестр правил нормирования закупок (РПНЗ)-->
	<xs:complexType name="zfcs_regulationRulesType">
		<xs:annotation>
