							<xs:documentation>Лот протокола</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="applications" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Заявки по лоту</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:sequence>
											<xs:element name="application" maxOccurs="unbounded">
