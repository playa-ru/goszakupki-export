																<xs:documentation>Результаты допуска заявки</xs:documentation>
															</xs:annotation>
														</xs:element>
														<xs:choice>
															<xs:element name="appRejectedReason" type="zfcs_appRejectedReasonNotIDType" maxOccurs="unbounded">
