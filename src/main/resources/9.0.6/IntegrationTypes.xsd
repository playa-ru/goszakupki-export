					<xs:documentation>ОГРН</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="taxRegDate" type="xs:date">
				<xs:annotation>
					<xs:documentation>Дата поставновки на учет в налоговом органе</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="address" type="zfcs_longTextMinType">
				<xs:annotation>
					<xs:documentation>Адрес места нахождения</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="addressPost" type="zfcs_longTextMinType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Почтовый адрес</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="phone" type="base:phoneType">
				<xs:annotation>
					<xs:documentation>Телефон</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="email" type="base:eMailType">
				<xs:annotation>
					<xs:documentation>Электронная почта</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="zfcs_repModificationInfo">
