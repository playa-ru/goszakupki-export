	<xs:complexType name="zfcs_nsiDrugChangeReason">
		<xs:annotation>
			<xs:documentation>Справочник: "Причины корректировки справочных данных о лекарственных препаратах"</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="code" type="base:text10Type">
				<xs:annotation>
					<xs:documentation>Код причины корректировки</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="name" type="base:text2000Type">
				<xs:annotation>
					<xs:documentation>Наименование причины корректировки.
Игнорируется при приеме.  При передаче заполняется значением из справочника "Причины корректировки справочных данных о лекарственных препаратах" (nsiDrugChangeReason)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="mustSpecifyCommentOrRequestNumber" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>Признак «Обязательно указание комментарий / номер обращения в службу тех поддержки»</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="mustSpecifyDrugRef" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>Признак «Обязательно указание ссылки на сведения о ЛП в ГРЛС»</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="isActual" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>Признак актуальности</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!--Планы-графики (РПГ)-->
	<xs:complexType name="zfcs_tenderPlanType">
		<xs:annotation>
			<xs:documentation>План-график в структурированной форме</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="commonInfo" type="zfcs_tenderPlanCommonInfoType">
				<xs:annotation>
					<xs:documentation>Общая информация плана-графика</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="customerInfo">
				<xs:annotation>
					<xs:documentation>Сведения о заказчике</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="customer" type="zfcs_purchaseOrganizationType">
							<xs:annotation>
								<xs:documentation>Организация заказчика</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="OKTMO" type="zfcs_OKTMORef">
							<xs:annotation>
								<xs:documentation>ОКТМО</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="responsibleContactInfo">
				<xs:annotation>
					<xs:documentation>Сведения об исполнителе (ответственном за формирование плана-графика)</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="zfcs_userTenderPlanType"/>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="providedPurchases" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Предусмотренные закупки </xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="positions" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Позиции плана-графика</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="position" type="zfcs_tenderPlanPositionType" maxOccurs="unbounded">
										<xs:annotation>
											<xs:documentation>Позиция плана-графика</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="extFillOrderNumber" type="xs:boolean" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Заполнение во внешней системе полей "Номер заказа (лота)" для всех позиций</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="finalPositions" type="zfcs_tenderPlanFinalPositionsType">
							<xs:annotation>
								<xs:documentation>Итоговые позиции</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="printForm" type="zfcs_printFormType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Печатная форма плана-графика</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="extPrintForm" type="zfcs_extPrintFormType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Электронный документ, полученный из внешней системы</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="schemeVersion" type="base:schemeVersionType" use="required">
			<xs:annotation>
				<xs:documentation>Версия схемы</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="zfcs_tenderPlanCancelType">
		<xs:annotation>
			<xs:documentation>Аннулирование плана-графика</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="id" type="xs:long" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Идентификатор аннулированного плана-графика на ЕИС</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="planNumber">
				<xs:annotation>
					<xs:documentation>Реестровый номер плана-графика</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="20"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="customerInfo" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Сведения о заказчике (для печатной формы)</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="zfcs_purchaseOrganizationType">
							<xs:sequence>
								<xs:element name="phone" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Телефон</xs:documentation>
									</xs:annotation>
									<xs:simpleType>
										<xs:restriction base="xs:string">
											<xs:maxLength value="256"/>
										</xs:restriction>
