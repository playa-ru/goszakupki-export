			<xs:element name="placerOrgInfo" type="zfcs_repPlacerOrgType">
				<xs:annotation>
					<xs:documentation>Организация, сформировавшая сведения</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="contractInfo" type="zfcs_repContractInfo">
				<xs:annotation>
					<xs:documentation>Информация о контракте</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="contractTerminationInfo" type="zfcs_repContractTerminationInfo" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Сведения о расторжении контракта</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="contractChangeInfo" type="zfcs_repContractChangeInfo" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Сведения об изменении условий контракта</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="supplierInfo" type="zfcs_repSupplierInfo">
				<xs:annotation>
					<xs:documentation>Информация о поставщике</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="modificationInfo" type="zfcs_repModificationInfo" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Информация о внесении изменений</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="zfcs_repContractInfo">
		<xs:annotation>
			<xs:documentation>Информация о контракте в реестровой записи РЕП</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="contractType">
				<xs:annotation>
					<xs:documentation>Вид контракта, заключенного с единственным поставщиком:
SP - Special – Специальный инвестиционный контракт;
ST - State – Закупка для обеспечения государственных нужд субъекта Российской Федерации.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="SP"/>
						<xs:enumeration value="ST"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="signedContractOrgInfo" type="zfcs_repSimpleOrganizationInfo">
				<xs:annotation>
					<xs:documentation>Организация, заключившая контракт</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="regulationPricesOrgInfo" type="zfcs_repSimpleOrganizationInfo">
				<xs:annotation>
					<xs:documentation>Организация, осуществляющая регулирование цен</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="contractDate" type="xs:date">
				<xs:annotation>
					<xs:documentation>Дата заключения контракта</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="contractNumber" type="zfcs_contract.Number2015Type" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Номер контракта, заключенного с единственным поставщиком
                    </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="contractDates" type="zfcs_repDates">
				<xs:annotation>
					<xs:documentation>Срок действия контракта</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="plannedDates" type="zfcs_repDates">
				<xs:annotation>
					<xs:documentation>Планируемый период, в течение которого заказчик вправе заключать контракт на поставку товара</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="factDates" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Фактический период, в течение которого заказчик вправе заключать контракт на  поставку товара</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="startDate" type="xs:date" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Дата начала</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="endDate" type="xs:date" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Дата окончания</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="investmentAmount" type="base:moneyPositiveType">
				<xs:annotation>
					<xs:documentation>Объем инвестиций, предусмотренных контрактом</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="contractNPAInfo" type="zfcs_repNPA">
				<xs:annotation>
					<xs:documentation>Нормативно-правовой акт</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="zfcs_repContractTerminationInfo">
		<xs:annotation>
			<xs:documentation>Сведения о расторжении контракта в реестровой записи РЕП</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="terminationDate" type="xs:date">
				<xs:annotation>
					<xs:documentation>Дата расторжения контракта с единственным поставщиком</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="terminationReasonText" type="zfcs_longTextMinType">
