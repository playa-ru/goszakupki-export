																<xs:annotation>
																	<xs:documentation>Сумма на второй плановый год</xs:documentation>
																</xs:annotation>
															</xs:element>
															<xs:element name="subsecYears" type="base:moneyMaxLengthToPoint18Type" minOccurs="0">
																<xs:annotation>
																	<xs:documentation>Сумма на последующие годы</xs:documentation>
																</xs:annotation>
															</xs:element>
														</xs:sequence>
													</xs:complexType>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
									<xs:element name="attachments" type="zfcs_attachmentListType" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Информация о прикрепленных документах</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="actInfo" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Сведения об объемах средств, указанных в правовых актах (проектах правовых актов), предусматривающих возможность заключения государственного (муниципального) контракта  на срок, превышающий срок действия доведенных лимитов бюджетных обязательств</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="docInfo">
							<xs:annotation>
								<xs:documentation>Сведения о НПА/ПА</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="docType">
										<xs:annotation>
											<xs:documentation>Вид документа</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="1"/>
												<xs:maxLength value="100"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="docNumber" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Номер документа</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="1"/>
												<xs:maxLength value="50"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="docDate" type="xs:date" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Дата документа (дата утверждения документа)</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="docName" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Наименование документа</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="1"/>
												<xs:maxLength value="1000"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="actFinanceInfo" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Объем средств, планируемых по НПА на осуществление закупки</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="KVR" type="zfcs_KVRRef">
										<xs:annotation>
											<xs:documentation>КВР</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="financeInfo">
										<xs:annotation>
											<xs:documentation>Объем средств</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:sequence>
												<xs:element name="total" type="base:moneyMaxLengthToPoint18Type" minOccurs="0">
													<xs:annotation>
														<xs:documentation>Всего. Значение игнорируется при приеме. автоматически рассчитывается как сумма нижеследующих полей (т.е. total=currentYear+firstYear+secondYear+subsecYears). </xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="currentYear" type="base:moneyMaxLengthToPoint18Type" minOccurs="0">
													<xs:annotation>
														<xs:documentation>Сумма на текущий плановый год</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="firstYear" type="base:moneyMaxLengthToPoint18Type" minOccurs="0">
													<xs:annotation>
														<xs:documentation>Сумма на первый плановый год</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="secondYear" type="base:moneyMaxLengthToPoint18Type" minOccurs="0">
													<xs:annotation>
														<xs:documentation>Сумма на второй плановый год</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="subsecYears" type="base:moneyMaxLengthToPoint18Type">
													<xs:annotation>
														<xs:documentation>Сумма на последующие годы</xs:documentation>
													</xs:annotation>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="attachments" type="zfcs_attachmentListType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Информация о прикрепленных документах</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	<xs:simpleType name="zfcs_purchasePlanAgreementTypeCodeEnum">
		<xs:annotation>
			<xs:documentation>Код типа соглашения о предоставлении субсидии: 
CAPITAL - На капитальные вложения;
OTHER - Иное.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
