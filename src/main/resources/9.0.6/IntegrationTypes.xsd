	<xs:complexType name="zfcs_control99ResponsibleType">
		<xs:annotation>
			<xs:documentation>Тип: Ответственный исполнитель для контроля по 99 статье</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="name" type="zfcs_control99HeadNameType">
