							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="subContractor" maxOccurs="unbounded">
										<xs:annotation>
											<xs:documentation>Субподрядчик, соисполнитель</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:choice>
