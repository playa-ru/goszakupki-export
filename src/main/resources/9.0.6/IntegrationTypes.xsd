						<xs:documentation>Классификация товара по ОКПД2</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="KTRU" type="zfcs_KTRURef">
					<xs:annotation>
						<xs:documentation>Классификация по КТРУ</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			<xs:element name="productName" type="zfcs_longTextMinType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Наименование товара, работы, услуги</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OKEI" type="zfcs_OKEIRef">
				<xs:annotation>
					<xs:documentation>Единица измерения</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="marginalUnitPrice" type="base:moneyPositiveType">
				<xs:annotation>
					<xs:documentation>Предельная цена за единицу</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="zfcs_repSimpleOrganizationInfo">
		<xs:annotation>
			<xs:documentation>Данные организации для Реестра единственных поставщиков</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="fullName" type="oos:zfcs_longTextMinType">
				<xs:annotation>
					<xs:documentation>Полное наименование</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="inn" type="base:innOrganizationType">
				<xs:annotation>
					<xs:documentation>ИНН организации</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="kpp" type="base:kppType">
				<xs:annotation>
					<xs:documentation>КПП организации</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="zfcs_repPlacerOrgType">
		<xs:annotation>
			<xs:documentation>Организация, разместившая сведения в реестровой записи РЕП</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="zfcs_repSimpleOrganizationInfo">
				<xs:sequence>
					<xs:element name="publishDate" type="xs:dateTime">
						<xs:annotation>
							<xs:documentation>Дата размещения сведений</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="zfcs_repSupplierInfo">
		<xs:annotation>
			<xs:documentation>Информация о поставщике в реестровой записи РЕП</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="fullName" type="zfcs_longTextMinType">
