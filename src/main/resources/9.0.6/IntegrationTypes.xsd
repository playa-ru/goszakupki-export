																<xs:sequence>
																	<xs:element name="correspondence" type="zfcs_applicationCorrespondence" maxOccurs="unbounded">
																		<xs:annotation>
																			<xs:documentation>Соответствие участника отдельному преимуществу/требованию к участникам </xs:documentation>
																		</xs:annotation>
																	</xs:element>
																</xs:sequence>
															</xs:complexType>
														</xs:element>
														<xs:element name="overallValue" minOccurs="0">
															<xs:annotation>
																<xs:documentation>Общая величина преимущества заявки</xs:documentation>
															</xs:annotation>
															<xs:simpleType>
																<xs:restriction base="xs:double">
																	<xs:minInclusive value="0"/>
																	<xs:maxInclusive value="100"/>
																</xs:restriction>
															</xs:simpleType>
														</xs:element>
														<xs:element name="offerPrice" type="base:moneyType" minOccurs="0">
															<xs:annotation>
																<xs:documentation>Предлагаемая цена (для печатной формы)</xs:documentation>
															</xs:annotation>
														</xs:element>
														<xs:element name="spelledOfferPrice" type="zfcs_longTextType" minOccurs="0">
															<xs:annotation>
