																</xs:annotation>
																<xs:simpleType>
																	<xs:restriction base="xs:string">
																		<xs:maxLength value="250"/>
																		<xs:minLength value="1"/>
																	</xs:restriction>
																</xs:simpleType>
															</xs:element>
															<xs:element name="INN" type="base:innType">
																<xs:annotation>
																	<xs:documentation>ИНН</xs:documentation>
																</xs:annotation>
															</xs:element>
															<xs:element name="address">
																<xs:annotation>
																	<xs:documentation>Адрес места нахождения</xs:documentation>
																</xs:annotation>
																<xs:simpleType>
																	<xs:restriction base="xs:string">
																		<xs:minLength value="1"/>
																		<xs:maxLength value="1024"/>
																	</xs:restriction>
																</xs:simpleType>
															</xs:element>
															<xs:element name="subContractInfo" type="zfcs_contract2015SubContractInfoType">
																<xs:annotation>
																	<xs:documentation>Информация о субподрядном логоворе</xs:documentation>
