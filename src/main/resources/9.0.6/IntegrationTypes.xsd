	<!--Загрузка лимитов бюджетных обязательств (ЛБО)-->
	<xs:complexType name="zfcs_LBOType">
		<xs:annotation>
			<xs:documentation>Сведения о лимитах бюджетных обязательств (ЛБО), загружаемых из региональных систем финансового учета</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="id" type="base:guidType">
				<xs:annotation>
					<xs:documentation>Идентификатор пакета</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="controlAuthorityInfo" type="base:organizationRef">
				<xs:annotation>
					<xs:documentation>Сведения об органе контроля. 
При приеме контролируется на наличие организации в справочнике "Сводный перечень заказчиков (СПЗ)" (nsiOrganizationList)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="customerLBOsInfo">
				<xs:annotation>
					<xs:documentation>Лимиты бюджетных обязательств (ЛБО) по заказчикам</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="customerLBOInfo" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>ЛБО по заказчику</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="createDataDT" type="xs:dateTime">
										<xs:annotation>
											<xs:documentation>Дата и время формирования сведений</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="relevanceYear" type="base:yearType">
										<xs:annotation>
											<xs:documentation>Год актуальности</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="name" type="base:text2000Type" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Наименование ЛБО</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="number" type="base:text2000Type" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Номер ЛБО</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="customerInfo">
										<xs:annotation>
											<xs:documentation>Организация заказчика. 
При приеме контролируется на наличие организации в справочнике "Сводный перечень заказчиков (СПЗ)" (nsiOrganizationList) и принадлежность к  органу контроля, указанному в блоке controlAuthorityInfo, в Перечне субъектов контроля (ПСК)</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:complexContent>
												<xs:extension base="base:organizationRef">
													<xs:sequence>
														<xs:element name="OKTMOPPO" type="base:OKTMOPPORef" minOccurs="0">
															<xs:annotation>
																<xs:documentation>Код ОКТМО ППО. При преме контролируется на наличие в справочнике "ОКТМО ППО" (nsiOKTMOPPO) </xs:documentation>
															</xs:annotation>
														</xs:element>
													</xs:sequence>
												</xs:extension>
