							<xs:sequence>
								<xs:element name="provideByContract" type="zfcs_contractExecutionType">
									<xs:annotation>
										<xs:documentation>Дата начала исполнения контракта (отдельного этапа исполнения контракта)</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="executed" type="zfcs_contractExecutionType">
									<xs:annotation>
										<xs:documentation>Дата окончания исполнения контракта (отдельного этапа исполнения контракта)</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="price">
									<xs:annotation>
										<xs:documentation>Цена контракта (отдельного этапа исполнения контракта): в валюте контракта в рублях</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:sequence>
											<xs:element name="priceByContract" type="base:moneyType">
