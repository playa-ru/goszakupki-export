						<xs:minLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="parentCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Код узла предка в иерархии</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="11"/>
						<xs:minLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="name" type="zfcs_longTextMinType">
				<xs:annotation>
					<xs:documentation>Наименование ППО</xs:documentation>
				</xs:annotation>
