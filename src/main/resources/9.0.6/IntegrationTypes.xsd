										</xs:restriction>
									</xs:simpleType>
								</xs:element>
								<xs:element name="position" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Должность</xs:documentation>
									</xs:annotation>
									<xs:simpleType>
										<xs:restriction base="xs:string">
