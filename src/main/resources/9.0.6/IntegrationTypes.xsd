															</xs:annotation>
														</xs:element>
														<xs:element name="unconsidered" type="xs:boolean" minOccurs="0">
															<xs:annotation>
																<xs:documentation>Заявка не рассмотрена (для печатной формы)</xs:documentation>
															</xs:annotation>
														</xs:element>
													</xs:sequence>
												</xs:complexType>
											</xs:element>
											<xs:element name="abandonedReason" type="zfcs_abandonedReasonType" minOccurs="0">
												<xs:annotation>
													<xs:documentation>Признание открытого конкурса несостоявшимся</xs:documentation>
												</xs:annotation>
											</xs:element>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="foundationProtocol" type="zfcs_foundationProtocolInfoType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Реквизиты предыдущего протокола</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="openingProtocol" type="zfcs_foundationProtocolInfoType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Реквизиты протокола вскрытия заявок (для печатной формы)</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
