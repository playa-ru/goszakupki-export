						<xs:annotation>
							<xs:documentation>Информация о предоставлении конкурсной документации</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="procedureInfo" type="zfcs_purchaseProcedureOKType">
						<xs:annotation>
							<xs:documentation>Информация о процедуре закупки</xs:documentation>
						</xs:annotation>
					</xs:element>
