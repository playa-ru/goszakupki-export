											<xs:element name="priceDocument" type="zfcs_longTextMinType">
												<xs:annotation>
													<xs:documentation>Документ подтверждающий исполнение</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="rejectionReason" type="zfcs_longTextMinType" minOccurs="0">
												<xs:annotation>
													<xs:documentation>Причина отклонения или неисполнения (в том числе причины отклонения от плана-графика)</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="priceNote" type="zfcs_longTextMinType" minOccurs="0">
												<xs:annotation>
													<xs:documentation>Примечание</xs:documentation>
												</xs:annotation>
											</xs:element>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="quantity">
									<xs:annotation>
										<xs:documentation>Количество (объем) поставляемых товаров, оказываемых услуг, выполняемых работ по контракту (по отдельному этапу исполнения контракта) с указанием единиц измерения (в случае если объект закупки может быть количественно измерен)</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:sequence>
											<xs:element name="quantityItem" minOccurs="0" maxOccurs="unbounded">
												<xs:annotation>
													<xs:documentation>Количество (объем)</xs:documentation>
												</xs:annotation>
												<xs:complexType>
													<xs:sequence>
														<xs:element name="OKEIByContract" type="zfcs_OKEIRef" minOccurs="0">
															<xs:annotation>
																<xs:documentation>Единица измерения контракта</xs:documentation>
															</xs:annotation>
														</xs:element>
														<xs:element name="quantityItemByContract" type="base:quantity18p11Type" minOccurs="0">
															<xs:annotation>
																<xs:documentation>Количество (объем),  предусмотренное контрактом</xs:documentation>
															</xs:annotation>
														</xs:element>
														<xs:element name="quantityByContract" type="base:moneyType" minOccurs="0">
															<xs:annotation>
																<xs:documentation>Количество (объем)  предусмотренное контрактом. Устарело, не применяется</xs:documentation>
															</xs:annotation>
														</xs:element>
														<xs:element name="OKEIExecution" type="zfcs_OKEIRef" minOccurs="0">
															<xs:annotation>
																<xs:documentation>Единица измерения исполнения</xs:documentation>
															</xs:annotation>
														</xs:element>
														<xs:element name="quantityItemExecution" type="base:quantity18p11Type" minOccurs="0">
															<xs:annotation>
																<xs:documentation>Количество (объем),  исполнено по контракту </xs:documentation>
															</xs:annotation>
														</xs:element>
														<xs:element name="quantityExecution" type="base:moneyType" minOccurs="0">
															<xs:annotation>
																<xs:documentation>Количество (объем)  исполнено по контракту.
Устарело, не применяется</xs:documentation>
															</xs:annotation>
														</xs:element>
													</xs:sequence>
												</xs:complexType>
											</xs:element>
											<xs:element name="quantityDocument" type="zfcs_longTextMinType">
												<xs:annotation>
													<xs:documentation>Документ подтверждающий исполнение</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="rejectionReason" type="zfcs_longTextMinType" minOccurs="0">
												<xs:annotation>
													<xs:documentation>Причина отклонения или неисполнения (в том числе причины отклонения от плана-графика)</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="quantityNote" type="zfcs_longTextMinType" minOccurs="0">
												<xs:annotation>
													<xs:documentation>Примечание</xs:documentation>
												</xs:annotation>
											</xs:element>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:choice>
									<xs:element name="advanceMarker" type="xs:boolean" fixed="false">
										<xs:annotation>
											<xs:documentation>Признак "Контрактом предусмотрена выплата аванса"</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="advance">
										<xs:annotation>
											<xs:documentation>Информация об авансовом платеже  вслучае, если контрактом предусмотрена выплата аванса</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:sequence>
												<xs:element name="advanceItem" maxOccurs="unbounded">
													<xs:annotation>
														<xs:documentation>Платеж</xs:documentation>
													</xs:annotation>
													<xs:complexType>
														<xs:sequence>
															<xs:element name="advanceByContract" type="base:moneyType">
																<xs:annotation>
																	<xs:documentation>Авансовый платеж (размер в рублях) предусмотрено контрактом</xs:documentation>
																</xs:annotation>
															</xs:element>
															<xs:element name="advanceDate" type="xs:date">
																<xs:annotation>
																	<xs:documentation>Дата перечисления аванса предусмотренная контрактом</xs:documentation>
																</xs:annotation>
															</xs:element>
															<xs:element name="advanceExecution" type="base:moneyType">
																<xs:annotation>
																	<xs:documentation>Авансовый платеж (размер в рублях) исполнено по контракту</xs:documentation>
																</xs:annotation>
															</xs:element>
															<xs:element name="executionDate" type="xs:date">
																<xs:annotation>
																	<xs:documentation>Дата исполнения перечисления аванса</xs:documentation>
																</xs:annotation>
															</xs:element>
														</xs:sequence>
													</xs:complexType>
												</xs:element>
												<xs:element name="advanceDocument" type="zfcs_longTextMinType">
													<xs:annotation>
														<xs:documentation>Документ подтверждающий исполнение</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="rejectionReason" type="zfcs_longTextMinType" minOccurs="0">
													<xs:annotation>
														<xs:documentation>Причина отклонения или неисполнения (в том числе причины отклонения от плана-графика)</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="advanceNote" type="zfcs_longTextMinType" minOccurs="0">
													<xs:annotation>
														<xs:documentation>Примечание к авансовому платежу</xs:documentation>
													</xs:annotation>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
								</xs:choice>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="improperExecutionInfo">
						<xs:annotation>
							<xs:documentation>Информация о ненадлежащем исполнении контракта или неисполнении контракта и о санкциях, связанных с указанным нарушением</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:choice>
								<xs:element name="conformContractMarker" type="xs:boolean" fixed="true">
									<xs:annotation>
										<xs:documentation>Признак "Исполнение контракта в соответствии с условиями"</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:sequence>
									<xs:element name="improperExecution" maxOccurs="unbounded">
										<xs:annotation>
											<xs:documentation>Ненадлежащее исполнение и (или) неисполнение заказчиком обязательств, предусмотренных контрактом, за исключением нарушения сроков исполнения обязательств</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:sequence>
												<xs:sequence>
													<xs:element name="indicator" type="zfcs_improperIndicatorTypeEnum">
														<xs:annotation>
															<xs:documentation>Код показателя
Допустимые значения:
Ro1 - Ненадлежащее исполнение и (или) неисполнение заказчиком обязательств, предусмотренных контрактом, за исключением нарушения сроков исполнения обязательств
Ro2 - Нарушение заказчиком сроков исполнения обязательств
Ro3 - Ненадлежащее исполнение и (или) неисполнение исполнителем обязательств, за исключением нарушений сроков исполнения обязательств, предусмотренных контрактом
Ro4 - Нарушение поставщиком (подрядчиком, исполнителем) сроков исполнения обязательств
Ro5 - Ненадлежащее исполнение обязательств банком, выдавшим исполнителю банковскую гарантию для целей обеспечения исполнения контракта</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="indicatorName" type="zfcs_improperIndicatorNameEnum" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Наименование показателя. Элемент не используется в импорте</xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
												<xs:element name="nameObligations" type="zfcs_longTextMinType">
													<xs:annotation>
														<xs:documentation>Наименование обязательства</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="essenceViolation" type="zfcs_longTextMinType">
													<xs:annotation>
														<xs:documentation>Суть нарушения</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="penaltyInfo" type="zfcs_longTextMinType">
													<xs:annotation>
														<xs:documentation>Информация о начисленной неустойке</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="penaltyDoc" type="zfcs_longTextMinType">
													<xs:annotation>
														<xs:documentation>Докмент, подтверждающий начисление или уплату неустойки</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="note" type="zfcs_longTextMinType" minOccurs="0">
													<xs:annotation>
														<xs:documentation>Примечание</xs:documentation>
													</xs:annotation>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:choice>
						</xs:complexType>
					</xs:element>
					<xs:element name="modifyTerminateInfo">
						<xs:annotation>
							<xs:documentation>Информация об изменении или о расторжении контракта в ходе его исполнения</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="modifyContract" minOccurs="0" maxOccurs="unbounded">
									<xs:annotation>
										<xs:documentation>Изменение контракта</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:complexContent>
											<xs:extension base="zfcs_modifyTerminateContractType">
												<xs:sequence>
													<xs:element name="reason">
														<xs:annotation>
															<xs:documentation>Причина </xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:element name="code" type="zfcs_nsiContractModificationReasonCodeType">
																	<xs:annotation>
																		<xs:documentation>Код
Справочник: Причины изменения условий контракта</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="name" type="zfcs_nsiContractModificationReasonNameType" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>Наименование причины. Элемент не используется в импорте</xs:documentation>
																	</xs:annotation>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
											</xs:extension>
										</xs:complexContent>
									</xs:complexType>
								</xs:element>
								<xs:element name="terminateContract" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Расторжение контракта</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:complexContent>
											<xs:extension base="zfcs_modifyTerminateContractType">
												<xs:sequence>
													<xs:element name="reason">
														<xs:annotation>
															<xs:documentation>Причина</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:element name="code">
																	<xs:annotation>
																		<xs:documentation>Код причины расторжения контракта в ОЗ.
Допустимые значения:
01 – по соглашению сторон;
02 – по решению суда;
03 – односторонний отказ заказчика от исполнения контракта;
04 – односторонний отказ исполнителя от исполнения контракта.</xs:documentation>
																	</xs:annotation>
																	<xs:simpleType>
																		<xs:restriction base="xs:string">
																			<xs:enumeration value="01"/>
																			<xs:enumeration value="02"/>
																			<xs:enumeration value="03"/>
																			<xs:enumeration value="04"/>
																		</xs:restriction>
																	</xs:simpleType>
																</xs:element>
																<xs:element name="name" type="zfcs_nsiContractTerminationReasonNameType" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>Наименование причины. Элемент не используется в импорте</xs:documentation>
																	</xs:annotation>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
											</xs:extension>
										</xs:complexContent>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="zfcs_customerReportContractExecutionInvalidType">
		<xs:annotation>
			<xs:documentation>Информация о недействительности сведений отчета об исполнении контракта (результатах отдельного этапа исполнения контракта)</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="zfcs_customerReportInvalidType">
				<xs:sequence>
					<xs:element name="report" type="zfcs_customerReportContractExecutionType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Информация отчета. Элемент не используется в импорте</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="zfcs_customerReportSmallScaleBusinessType">
		<xs:annotation>
			<xs:documentation>Отчет об объеме закупок у СМП (субъектов малого предпринимательства), СОНО (социально ориентированных некоммерческих организаций)</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="zfcs_customerReportType">
				<xs:sequence>
					<xs:element name="customer" type="zfcs_organizationInfoExtendedType">
						<xs:annotation>
							<xs:documentation>Информация о заказчике</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="reportingPeriod" type="zfcs_yearType">
						<xs:annotation>
							<xs:documentation>Отчетный период</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="quantityPurchase">
						<xs:annotation>
							<xs:documentation>Информация об объёме закупок</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="privacy" type="zfcs_volumeType" minOccurs="0">
									<xs:annotation>
										<xs:documentation>1.1. Совокупный годовой объем закупок, за исключением объема закупок, сведения о которых составляют государственную тайну</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="annualVolumeSt30" type="zfcs_volumeType" minOccurs="0">
									<xs:annotation>
										<xs:documentation>1.2. Общий объем финансового обеспечения для оплаты контрактов в отчетном году в рамках осуществления закупок, предусмотренных частью 1.1 статьи 30 Федерального закона "О контрактной системе в сфере закупок товаров, работ, услуг для обеспечения государственных и муниципальных нужд" (далее - Федеральный закон), за исключением объема финансового обеспечения для оплаты в отчетном году контрактов, содержащих сведения, составляющие государственную тайну  (тыс. руб.). Элемент не используется в импорте, значение рассчитывается по сумме последующих полей</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="defending" type="zfcs_volumeType" minOccurs="0">
									<xs:annotation>
										<xs:documentation>1.2.1. Объем финансового обеспечения для оплаты в отчетном году контрактов, заключаемых для обеспечения обороны страны и безопасности государства, за исключением объема финансового обеспечения для оплаты в отчетном году контрактов, содержащих сведения, составляющие государственную тайну (тыс. руб.)</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="lending" type="zfcs_volumeType" minOccurs="0">
									<xs:annotation>
										<xs:documentation>1.2.2. Объем финансового обеспечения для оплаты в отчетном году контрактов, заключаемых на оказание услуг по предоставлению кредитов, за исключением объема финансового обеспечения для оплаты в отчетном году контрактов, содержащих сведения, составляющие государственную тайну (тыс. руб.)</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="singleSupplier" type="zfcs_volumeType" minOccurs="0">
									<xs:annotation>
										<xs:documentation>1.2.3. Объем финансового обеспечения для оплаты в отчетном году контрактов, заключаемых с единственным поставщиком (подрядчиком, исполнителем)  в соответствии с частью 1 статьи 93  Федерального закона, за исключением объема финансового обеспечения для оплаты в отчетном году контрактов, содержащих сведения, составляющие государственную тайну  (тыс. руб.)</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="nuclearEnergy" type="zfcs_volumeType" minOccurs="0">
									<xs:annotation>
										<xs:documentation>1.2.4. Объем финансового обеспечения для оплаты в отчетном году контрактов, заключаемых на выполнение работ в области использования атомной энергии, за исключением объема финансового обеспечения для оплаты в отчетном году контрактов, содержащих сведения, составляющие государственную тайну  (тыс. руб.)</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="ZK" type="zfcs_volumeType" minOccurs="0">
									<xs:annotation>
										<xs:documentation>1.2.5 Объем финансового обеспечения для оплаты в отчетном году контрактов, заключаемых по результатам закрытых способов определения поставщиков (подрядчиков, исполнителей), за исключением объема финансового обеспечения для оплаты в отчетном году контрактов, содержащих сведения, составляющие государственную тайну  (тыс. руб.)</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="annualVolume" type="zfcs_volumeType" minOccurs="0">
									<xs:annotation>
										<xs:documentation>1.3. Совокупный годовой объем закупок, рассчитанный за вычетом закупок, предусмотренных частью 1.1 статьи 30 Федерального закона (тыс. руб.). Элемент не используется в импорте
, значение рассчитывается по разнице полей 1.2 и 1.1</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="percent15" type="zfcs_volumeType" minOccurs="0">
									<xs:annotation>
										<xs:documentation>1.4. Объем закупок, который заказчик обязан осуществить у СМП, СОНО в отчетном году (не менее чем 15 процентов совокупного годового объема закупок, рассчитанного с учетом части 1.1   статьи 30 Федерального закона) (тыс. руб.)</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="onlySMP" type="zfcs_volumeType" minOccurs="0">
									<xs:annotation>
										<xs:documentation>2.1. Объем закупок в отчетном году, осуществленных по результатам состоявшихся процедур определения поставщика (подрядчика, исполнителя), в извещении об осуществлении которых было установлено ограничение в отношении участников закупок, которыми могли быть только СМП, СОНО (тыс. руб.)</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="notSMP" type="zfcs_volumeType" minOccurs="0">
									<xs:annotation>
										<xs:documentation>2.2. Объем привлечения в отчетном году субподрядчиков, соисполнителей из числа СМП, СОНО к исполнению контрактов, заключенных по результатам определений поставщиков (подрядчиков, исполнителей), в извещениях об осуществлении которых было установлено требование к поставщику (подрядчику, исполнителю), не являющемуся СМП, СОНО о привлечении к исполнению контракта субподрядчиков, соисполнителей из числа СМП, СОНО (тыс. руб.)</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="annualVolumeSMP" type="zfcs_volumeType" minOccurs="0">
									<xs:annotation>
										<xs:documentation>2.3. Объем закупок, который заказчик осуществил у СМП, СОНО в отчетном году (тыс. руб.). Элемент не используется в импорте
, значение рассчитывается по сумме полей 2.1 и 2.2</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="rateSMP" type="zfcs_volumeType" minOccurs="0">
									<xs:annotation>
										<xs:documentation>2.4. Доля закупок, которые заказчик осуществил у СМП, СОНО в отчетном году, в совокупном годовом объеме закупок, рассчитанном за вычетом закупок, предусмотренных частью 1.1 статьи 30 Федерального закона (%)/ Элемент не используется в импорте
, значение рассчитывается по отношению полей 2.3 к 1.3</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="abandonedSum" type="zfcs_volumeType" minOccurs="0">
									<xs:annotation>
										<xs:documentation>3.1. Сумма начальных (максимальных) цен контрактов несостоявшихся определений поставщиков (подрядчиков, исполнителей) с участием СМП, СОНО (тыс. руб.)</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="contractsInfo">
						<xs:annotation>
							<xs:documentation>Информация о заключенных контрактах</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="contractInfo" minOccurs="0" maxOccurs="unbounded">
									<xs:annotation>
										<xs:documentation>Информация о заключенном контракте</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:sequence>
											<xs:element name="contractRegNum" type="zfcs_contract.regNumType">
												<xs:annotation>
													<xs:documentation>Реестровый номер контракта</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="contractNumber" type="zfcs_contract.NumberType" minOccurs="0">
												<xs:annotation>
													<xs:documentation>Номер контракта. Элемент не используется в импорте</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="contractType" type="zfcs_customerReportContractTypeEnum">
												<xs:annotation>
													<xs:documentation>Тип контракта
Допустимые значения:
-SMP - Контракты, заключенные с субъектами малого предпринимательства, социально ориентированными некоммерческими организациями;
-CON - Контракты, содержащие условие о привлечении к исполнению контрактов субподрядчиков, соисполнителей из числа субъектов малого предпринимательства, социально ориентированных некоммерческих организаций;
-S30 - Контракты, заключенные по основаниям, предусмотренным частью 1.1 статьи 30 Федерального закона;
-CRD - Контракты на оказание услуг по предоставлению кредитов;
-SGL - контракты, заключенные с единственным поставщиком (подрядчиком, исполнителем) в соответствии с частью 1 статьи 93 Федерального закона;
-ATM - контракты на выполнение работ в области использования атомной энергии;
-ZK - контракты, при осуществлении которых применяются закрытые способы определения поставщиков (подрядчиков, исполнителей).

</xs:documentation>
												</xs:annotation>
											</xs:element>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="zfcs_customerReportSmallScaleBusinessInvalidType">
		<xs:annotation>
			<xs:documentation>Информация о недействительности сведений отчета об объеме закупок у СМП (субъектов малого предпринимательства), СОНО (социально ориентированных некоммерческих организаций)</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="zfcs_customerReportInvalidType">
				<xs:sequence>
					<xs:element name="report" type="zfcs_customerReportSmallScaleBusinessType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Информация отчета. Элемент не используется в импорте</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="zfcs_customerReportSingleContractorType">
		<xs:annotation>
			<xs:documentation>Отчет с обоснованием закупки у единственного поставщика (подрядчика, исполнителя)</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="zfcs_customerReportType">
				<xs:sequence>
					<xs:element name="customer" type="zfcs_organizationInfoWithOgrnType">
						<xs:annotation>
							<xs:documentation>Информация о заказчике</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="purchaseNumber" type="base:purchaseNumberType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Номер извещения об осуществлении закупки</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="contractRegNum" type="zfcs_contract.regNumType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Реестровый номер контракта</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="lotNumber" type="xs:integer" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Номер лота в извещении</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="reason">
						<xs:annotation>
							<xs:documentation>Основание заключения контракта с единственным поставщиком</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="code" type="zfcs_nsiSingleCustomerReasonCodeType">
									<xs:annotation>
										<xs:documentation>Код</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="name" type="zfcs_nsiSingleCustomerReasonNameType" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Наименование основания</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="zfcs_customerReportSingleContractorInvalidType">
		<xs:annotation>
			<xs:documentation>Информация о недействительности сведений отчета с обоснованием закупки у единственного поставщика (подрядчика, исполнителя)</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="zfcs_customerReportInvalidType">
				<xs:sequence>
					<xs:element name="report" type="zfcs_customerReportSingleContractorType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Информация отчета. Элемент не используется в импорте</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="zfcs_customerReportBigProjectMonitoringType">
		<xs:annotation>
			<xs:documentation>Отчет по мониторингу реализации крупных проектов с государственным участием</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="zfcs_customerReportType">
				<xs:sequence>
					<xs:element name="needInContract" type="xs:boolean">
						<xs:annotation>
							<xs:documentation>Для реализации инвестиционного проекта должен заключаться государственный контракт</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="customer" type="zfcs_organizationInfoWithOgrnType">
						<xs:annotation>
							<xs:documentation>Информация о заказчике</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="contractsInfo" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Информация о заключенных контрактах. Указывается если государственные контракты размещены на Официальном сайте </xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="contractInfo" maxOccurs="unbounded">
									<xs:annotation>
										<xs:documentation>Информация о заключенном контракте</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:sequence>
											<xs:element name="contractRegNum" type="zfcs_contract.regNumType">
												<xs:annotation>
													<xs:documentation>Реестровый номер контракта</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="contractNumber" type="zfcs_contract.NumberType" minOccurs="0">
												<xs:annotation>
													<xs:documentation>Номер контракта. Элемент не используется в импорте</xs:documentation>
												</xs:annotation>
											</xs:element>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="constructor" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Сведения о застройщике или заказчике</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="fullName" type="zfcs_longTextMinType" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Полное наименование</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="shortName" type="zfcs_longTextMinType" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Сокращенное наименование</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="OKOPF" type="zfcs_okopfRef" minOccurs="0">
									<xs:annotation>
										<xs:documentation>ОКОПФ организации</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="head" type="zfcs_longTextMinType" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Ф.И.О. руководителя</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="position" type="zfcs_longTextMinType" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Должность руководителя </xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="projectInfo">
						<xs:annotation>
							<xs:documentation>Общая информация об инвестиционном проекте</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="name" type="zfcs_longTextMinType">
									<xs:annotation>
										<xs:documentation>Наименование инвестиционного проекта</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="purpose" type="zfcs_longTextMinType">
									<xs:annotation>
										<xs:documentation>Цель инвестиционного проекта</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="period">
									<xs:annotation>
										<xs:documentation>Срок реализации инвестиционного проекта</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:sequence>
											<xs:element name="startYear" type="zfcs_yearType">
												<xs:annotation>
													<xs:documentation>Год начала</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="endYear" type="zfcs_yearType">
												<xs:annotation>
													<xs:documentation>Год окончания</xs:documentation>
												</xs:annotation>
											</xs:element>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="direction" maxOccurs="unbounded">
									<xs:annotation>
										<xs:documentation>Направление реализации инвестиционного проекта.
Допустимые значения:
IT1 - строительство;
IT2 - реконструкция объекта капитального строительства;
IT3 - иные инвестиции в основной капитал.</xs:documentation>
									</xs:annotation>
									<xs:simpleType>
										<xs:restriction base="xs:string">
											<xs:enumeration value="IT1"/>
											<xs:enumeration value="IT2"/>
											<xs:enumeration value="IT3"/>
										</xs:restriction>
									</xs:simpleType>
								</xs:element>
								<xs:element name="otherDirection" type="zfcs_longTextMinType" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Иной вид инвестиции в основной капитал. Указывается если выбран 3 тип направления реализации инвестиционного проекта</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="mechanism">
									<xs:annotation>
										<xs:documentation>Механизм, в рамках которого представляются средства федерального бюджета.
Допустимые значения:
FP1 - федеральная инвестиционная программа,
FP2 - Инвестиционный фонд Российской Федерации,
FP3 - Фонд национального благосостояния,
FP4 - иной механизм с его указанием</xs:documentation>
									</xs:annotation>
									<xs:simpleType>
										<xs:restriction base="xs:string">
											<xs:enumeration value="FP1"/>
											<xs:enumeration value="FP2"/>
											<xs:enumeration value="FP3"/>
											<xs:enumeration value="FP4"/>
										</xs:restriction>
									</xs:simpleType>
								</xs:element>
								<xs:element name="otherMechanism" type="zfcs_longTextMinType" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Иной механизм, в рамках которого представляются средства федерального бюджета. Указывается если выбран 4 тип направления реализации инвестиционного проекта</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="grbs" type="zfcs_longTextMinType">
									<xs:annotation>
										<xs:documentation>Главный распорядитель средств федерального бюджета</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="indicators">
									<xs:annotation>
										<xs:documentation>Количественные показатели (показатель) результатов реализации инвестиционного проекта</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:sequence>
											<xs:element name="indicator" maxOccurs="unbounded">
												<xs:annotation>
													<xs:documentation>Показатель</xs:documentation>
												</xs:annotation>
												<xs:complexType>
													<xs:sequence>
														<xs:element name="name" type="zfcs_longTextMinType">
															<xs:annotation>
																<xs:documentation>Наименование показателя</xs:documentation>
															</xs:annotation>
														</xs:element>
														<xs:element name="value" type="zfcs_longTextMinType">
															<xs:annotation>
																<xs:documentation>Значение показателя по результатам реализации</xs:documentation>
															</xs:annotation>
														</xs:element>
													</xs:sequence>
												</xs:complexType>
											</xs:element>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="contractors">
						<xs:annotation>
							<xs:documentation>Основные субподрядчики, привлеченные к реализации инвестиционного проекта (объекта), со стоимостью договора или договоров более чем 10 процентов от цены контракта</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="contractor" type="zfcs_bigProjectMemberType" maxOccurs="unbounded">
									<xs:annotation>
										<xs:documentation>Субподрядчик</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="participants" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Участники инвестиционного проекта</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="participant" type="zfcs_bigProjectMemberType" maxOccurs="unbounded">
									<xs:annotation>
										<xs:documentation>Участник</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="documents">
						<xs:annotation>
							<xs:documentation>Документы</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="projectDocumentation">
									<xs:annotation>
										<xs:documentation>Проектная документация по инвестиционному проекту</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:choice>
											<xs:element name="presence" type="xs:boolean" fixed="false">
												<xs:annotation>
													<xs:documentation>Наличие проектной документации по инвестиционному проекту</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="documentation" type="zfcs_longTextMinType">
												<xs:annotation>
													<xs:documentation>Ссылка на подтверждающий документ проектной документации по инвестиционному проекту. Указывается при наличии проектной документации </xs:documentation>
												</xs:annotation>
											</xs:element>
										</xs:choice>
									</xs:complexType>
								</xs:element>
								<xs:element name="statement">
									<xs:annotation>
										<xs:documentation>Положительное заключение государственной экспертизы проектной документации и результаты инженерных изысканий</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:choice>
											<xs:element name="presence" type="xs:boolean" fixed="false">
												<xs:annotation>
													<xs:documentation>Наличие положительного заключения государственной экспертизы проектной документации и результатов инженерных изысканий</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:sequence>
												<xs:element name="copy" type="zfcs_attachmentListType">
													<xs:annotation>
														<xs:documentation>Копия</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="document" type="zfcs_longTextMinType">
													<xs:annotation>
														<xs:documentation>Ссылка на документ</xs:documentation>
													</xs:annotation>
												</xs:element>
											</xs:sequence>
										</xs:choice>
									</xs:complexType>
								</xs:element>
								<xs:element name="examination">
									<xs:annotation>
										<xs:documentation>Положительное заключение о достоверности сметной стоимости инвестиционного проекта (объекта)</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:choice>
											<xs:element name="presence" type="xs:boolean" fixed="false">
												<xs:annotation>
													<xs:documentation>Наличие положительного заключения о достоверности сметной стоимости инвестиционного проекта (объекта)</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:sequence>
												<xs:element name="copy" type="zfcs_attachmentListType">
													<xs:annotation>
														<xs:documentation>Копия</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="document" type="zfcs_longTextMinType">
													<xs:annotation>
														<xs:documentation>Ссылка на документ</xs:documentation>
													</xs:annotation>
												</xs:element>
											</xs:sequence>
										</xs:choice>
									</xs:complexType>
								</xs:element>
								<xs:element name="audit">
									<xs:annotation>
										<xs:documentation>Положительное заключение о проведении публичного технологического и ценового аудита инвестиционного проекта, в случае если его проведение предусмотрено законодательством Российской Федерации</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:choice>
											<xs:element name="presence" type="xs:boolean" fixed="false">
												<xs:annotation>
													<xs:documentation>Наличие положительного сводного заключения о проведении публичного технологического и ценового аудита инвестиционного проекта, в случае если его проведение предусмотрено законодательством Российской Федерации</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:sequence>
												<xs:element name="copy" type="zfcs_attachmentListType">
													<xs:annotation>
														<xs:documentation>Копия</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="document" type="zfcs_longTextMinType">
													<xs:annotation>
														<xs:documentation>Ссылка на документ</xs:documentation>
													</xs:annotation>
												</xs:element>
											</xs:sequence>
										</xs:choice>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="cost">
						<xs:annotation>
							<xs:documentation>Стоимость инвестиционного проекта (объекта), в млн. рублей</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="act">
									<xs:annotation>
										<xs:documentation>В соответствии с актом о предоставлении средств федерального бюджета на реализацию инвестиционного проекта (в случае если принятие такого акту предусмотрено Российской Федерацией), актом об утверждении паспорта инвестиционного проекта, соглашением о предоставлении средств федерального бюджета, рассчитанная в ценах соответствующих лет</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:sequence>
											<xs:element name="priceYear" type="zfcs_bigProjectCostType" maxOccurs="unbounded">
												<xs:annotation>
													<xs:documentation>Год</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="vat" type="xs:boolean">
												<xs:annotation>
													<xs:documentation>Включая НДС / без НДС </xs:documentation>
												</xs:annotation>
											</xs:element>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="authenticity">
									<xs:annotation>
										<xs:documentation>В соответствии с заключением о достоверности сметной стоимости инвестиционного проекта (объекта) в ценах года ее определения</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:complexContent>
											<xs:extension base="zfcs_bigProjectCostType">
												<xs:sequence>
													<xs:element name="priceYear" type="zfcs_bigProjectCostType" maxOccurs="unbounded">
														<xs:annotation>
															<xs:documentation>Год</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="vat" type="xs:boolean">
														<xs:annotation>
															<xs:documentation>Включая НДС / без НДС </xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
											</xs:extension>
										</xs:complexContent>
									</xs:complexType>
								</xs:element>
								<xs:element name="contract">
									<xs:annotation>
										<xs:documentation>По результатам заключения государственного контракта (договора на подрядные работы) по инвестиционному проекту с указанием сроков его исполнения </xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:sequence>
											<xs:element name="startYear" type="zfcs_yearType">
												<xs:annotation>
													<xs:documentation>Год начала исполнения</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="endYear" type="zfcs_yearType">
												<xs:annotation>
													<xs:documentation>Год окончания исполнения</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="vat" type="xs:boolean">
												<xs:annotation>
													<xs:documentation>Включая НДС / без НДС </xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="cost" type="zfcs_longTextMinType">
												<xs:annotation>
													<xs:documentation>Стоимость инвестиционного проекта, рассчитанная в ценах соответствующих лет</xs:documentation>
												</xs:annotation>
											</xs:element>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="economy" type="base:moneyPositiveType">
									<xs:annotation>
										<xs:documentation>Экономия по результатам проведения конкурсных процедур по заключению государственного контракта (договора) на реализацию инвестиционного проекта, в млн. рублей</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="financings">
						<xs:annotation>
							<xs:documentation>Источники и объемы финансирования инвестиционного проекта в ценах соответствующих лет</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="years" type="zfcs_bigProjectFinancingYearsType">
									<xs:annotation>
										<xs:documentation>Годы реализации инвестиционного проекта </xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="stages">
									<xs:annotation>
										<xs:documentation>Этапы реализации инвестиционного проекта</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:sequence>
											<xs:element name="stage" maxOccurs="unbounded">
												<xs:annotation>
													<xs:documentation>Этап</xs:documentation>
												</xs:annotation>
												<xs:complexType>
													<xs:sequence>
														<xs:element name="number" type="xs:positiveInteger">
															<xs:annotation>
																<xs:documentation>Номер этапа </xs:documentation>
															</xs:annotation>
														</xs:element>
														<xs:element name="years" type="zfcs_bigProjectFinancingYearsType">
															<xs:annotation>
																<xs:documentation>Годы этапа</xs:documentation>
															</xs:annotation>
														</xs:element>
													</xs:sequence>
												</xs:complexType>
											</xs:element>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="site" type="zfcs_longTextMinType">
									<xs:annotation>
										<xs:documentation>Адрес сайта в информационно-телекоммуникационной сети "Интернет", на котором размещена информация о реализации инвестиционного проекта </xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
