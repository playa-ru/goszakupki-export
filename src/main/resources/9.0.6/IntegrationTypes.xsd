												<xs:element name="documentInfoList" type="zfcs_penalty.documentInfoList">
													<xs:annotation>
														<xs:documentation>Сведения о документах, подтверждающих отмену или изменение меры взыскания неустойки</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="penaltyChangeFact" minOccurs="0">
													<xs:annotation>
														<xs:documentation>Информация о факте отмены (возврата) неустойки</xs:documentation>
													</xs:annotation>
													<xs:complexType>
														<xs:sequence>
															<xs:element name="documentInfoList" type="zfcs_penalty.documentInfoList">
																<xs:annotation>
																	<xs:documentation>Сведения о документах, подтверждающих факт отмены (возврата) неустойки</xs:documentation>
																</xs:annotation>
															</xs:element>
															<xs:element name="returnedAmount" type="base:moneyPositiveType">
																<xs:annotation>
																	<xs:documentation>Размер возвращенной плательщику суммы (в рублях)</xs:documentation>
																</xs:annotation>
															</xs:element>
															<xs:element name="returnedAmountInPercents" minOccurs="0">
																<xs:annotation>
																	<xs:documentation>Размер возвращенной плательщику суммы в % от фактически взысканного</xs:documentation>
																</xs:annotation>
																<xs:simpleType>
																	<xs:restriction base="xs:double">
																		<xs:minInclusive value="0"/>
																	</xs:restriction>
																</xs:simpleType>
															</xs:element>
														</xs:sequence>
													</xs:complexType>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="printForm" type="zfcs_contract.printFormType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Печатная форма информации о неустойках (штрафах, пенях)</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="printForm" type="zfcs_contract.printFormType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Печатная форма информации об исполнении (расторжении) контракта</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="paymentDocuments" type="zfcs_contract.attachmentListType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Документы, подтверждающие исполнение, оплату</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="receiptDocuments" type="zfcs_contract.attachmentListType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Документы, подтверждающие приемку товара</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="productOriginDocuments" type="zfcs_contract.attachmentListType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Информация о стране происхождения товара</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="reason" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Основание изменения (исправления) опубликованной редакции. Указывается в случае передачи измененных сведений</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="4000"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="currentContractStage" type="zfcs_contract.contractStageType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Текущее состояние контракта:
E 	Исполнение;
ET	Исполнение прекращено;
EC	Исполнение завершено;
IN	Aннулировано.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="schemeVersion" type="zfcs_schemeVersionContractType" use="required">
			<xs:annotation>
				<xs:documentation>Версия схемы</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="zfcs_contractProcedureCancelType">
		<xs:annotation>
			<xs:documentation>Сведения об отмене информации об исполнении (расторжении) контракта</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="cancelledProcedureId">
				<xs:annotation>
					<xs:documentation>Идентификатор отменяемой информации об исполнении (расторжении) контракта</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:long"/>
				</xs:simpleType>
			</xs:element>
			<xs:element name="regNum" type="zfcs_contract.regNumType">
				<xs:annotation>
					<xs:documentation>Номер реестровой записи государственного или муниципального контракта</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="cancelDate" type="xs:dateTime">
				<xs:annotation>
					<xs:documentation>Дата отмены информации об исполнении (расторжении) контракта</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="reason">
				<xs:annotation>
					<xs:documentation>Основание отмены информации об исполнении (расторжении) контракта</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="4000"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="currentContractStage" type="zfcs_contract.contractStageType">
				<xs:annotation>
					<xs:documentation>Текущее состояние контракта:
E 	Исполнение;
ET	Исполнение прекращено;
EC	Исполнение завершено;
IN	Aннулировано.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="schemeVersion" type="zfcs_schemeVersionContractType" use="required">
			<xs:annotation>
				<xs:documentation>Версия схемы</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="zfcs_contractSignType">
		<xs:annotation>
			<xs:documentation>Информация о подписании государственного/муниципального контракта на ЭП</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="id" type="xs:long" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Идентификатор документа ЕИС</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="number" type="zfcs_contract.NumberType">
				<xs:annotation>
					<xs:documentation>Номер контракта</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="signDate" type="xs:date">
				<xs:annotation>
					<xs:documentation>Дата заключения контракта</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="foundation">
				<xs:annotation>
					<xs:documentation>Основание заключения контракта</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="order">
							<xs:annotation>
								<xs:documentation>Заказ размещался в ЕИС</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="purchaseNumber" type="base:purchaseNumberType">
										<xs:annotation>
											<xs:documentation>Номер закупки</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="foundationProtocolNumber" type="zfcs_documentNumberType" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Номер протокола-основания</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<!--xs:element name="реквизиты письма КО с согласованием размещения у единственного поставщика, бывают случаи просто указания на пункты 94-ФЗ"/-->
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="customer" type="zfcs_organizationRef">
				<xs:annotation>
					<xs:documentation>Заказчик</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="price" type="base:moneyPositiveType">
				<xs:annotation>
					<xs:documentation>Цена контракта
(в валюте контракта)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="priceRUR" type="base:moneyPositiveType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Цена контракта (в рублях)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="currency" type="zfcs_currencyRef">
				<xs:annotation>
					<xs:documentation>Валюта контракта</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="concludeContractRight" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>Признак аукциона на право заключить контракт</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="protocolDate" type="xs:date">
				<xs:annotation>
					<xs:documentation>Дата подведения итогов электронного аукциона</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="suppliers">
				<xs:annotation>
					<xs:documentation>Поставщики</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="supplier" type="zfcs_participantType" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="document" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Документ, направляемый от электронной площадки на ЕИС в рамках реализации статьи 70 Федерального закона № 44ФЗ</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="publishedContentId" type="base:guidType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Уникальный идентификатор контента прикрепленного документа на ЕИС </xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="documentType" type="zfcs_documentType70ArticleType">
							<xs:annotation>
								<xs:documentation>Тип документа:

CPWOS - Проект контракта без подписей, направляемый победителю аукциона;
PDWWS - Протокол разногласий с подписью победителя аукциона;
RRWCS -  Документ с причинами отказа учета замечаний в протоколе разногласий с подписью заказчика и (или) Проект контракта, направляемый победителю аукциона
CPWWS - Проект контракта с подписью победителя аукциона
ECWWS -  Предоставление обеспечения исполнения контракта с подписью победителя аукциона
CPWWCS - Подписанный контракт (с подписью заказчика и победителя аукциона)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="fileName">
							<xs:annotation>
								<xs:documentation>Имя файла</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="1024"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="fileSize" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Размер файла</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="40"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="docDescription" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Описание прикрепляемого документа</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="1024"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:choice>
							<xs:element name="url">
								<xs:annotation>
									<xs:documentation>Ссылка для скачивания прикрепленного документа</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:maxLength value="1024"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="contentId" type="base:guidType">
								<xs:annotation>
									<xs:documentation>Уникальный идентификатор контента прикрепленного документа на ЕИС</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="content" type="xs:base64Binary">
								<xs:annotation>
									<xs:documentation>Содержимое файла</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:choice>
						<xs:element name="customerSignature" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Электронная подпись заказчика</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:simpleContent>
									<xs:extension base="xs:base64Binary">
										<xs:attribute name="type" type="base:signatureType">
											<xs:annotation>
												<xs:documentation>Тип электронной подписи:
CAdES-BES;
CAdES-A</xs:documentation>
											</xs:annotation>
										</xs:attribute>
									</xs:extension>
								</xs:simpleContent>
							</xs:complexType>
						</xs:element>
						<xs:element name="supplierSignature" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Электронная подпись поставщика (победителя аукциона)</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:simpleContent>
									<xs:extension base="xs:base64Binary">
										<xs:attribute name="type" type="base:signatureType">
											<xs:annotation>
												<xs:documentation>Тип электронной подписи:
CAdES-BES;
CAdES-A</xs:documentation>
											</xs:annotation>
										</xs:attribute>
									</xs:extension>
								</xs:simpleContent>
							</xs:complexType>
						</xs:element>
						<xs:element name="controlPersonalSignature" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Электронная подпись лица, уполномоченного на проведение контроля в соответствии с ч.5 ст.99 закона №44-ФЗ</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:simpleContent>
									<xs:extension base="xs:base64Binary">
										<xs:attribute name="type" type="base:signatureType">
											<xs:annotation>
												<xs:documentation>Тип электронной подписи:
CAdES-BES;
CAdES-A</xs:documentation>
											</xs:annotation>
										</xs:attribute>
									</xs:extension>
								</xs:simpleContent>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="schemeVersion" type="base:schemeVersionType" use="required">
			<xs:annotation>
				<xs:documentation>Версия схемы</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="zfcs_contractMultiType">
		<xs:annotation>
			<xs:documentation>Право заключения контракта с несколькими участниками закупки</xs:documentation>
		</xs:annotation>
		<xs:choice>
			<xs:element name="contractCount" type="xs:positiveInteger" minOccurs="0">
