					<xs:documentation>Реестровый номер записи РЕП</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="createDate" type="xs:dateTime">
				<xs:annotation>
					<xs:documentation>Дата/время формирования уведомления </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="zfcs_repAutoNotPassProtocolType">
		<xs:annotation>
			<xs:documentation>Протокол о непрохождении автоматической проверки соблюдения правил формирования и направления информации в РЕП</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="notIncludedReasonsText">
