	<!--Недобросовестные поставщики (РНП)-->
	<xs:complexType name="zfcs_unfairSupplierType">
		<xs:annotation>
			<xs:documentation>Сведения о недобросовестном поставщике</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="registryNum" type="zfcs_documentNumberType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Реестровый номер</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="publishDate" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Дата публикации</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="approveDate" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Дата включения сведений в реестр</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="state">
				<xs:annotation>
					<xs:documentation>Статус информации
REVIEW_CANCELED - Отклонена (Отказ во включении в РНП);
PUBLISHED - Опубликована;
ON_EXCLUSION - Заявка на исключение сведений;
TEMP_EXCLUDED - Информация исключена из РНП на время судебного разбирательства;
ARCHIVED - Информация исключена из РНП. Архив;</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="20"/>
						<xs:enumeration value="REVIEW_CANCELED"/>
						<xs:enumeration value="PUBLISHED"/>
						<xs:enumeration value="ON_EXCLUSION"/>
						<xs:enumeration value="TEMP_EXCLUDED"/>
						<xs:enumeration value="ARCHIVED"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="publishOrg" type="zfcs_organizationRef" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Уполномоченный орган, осуществивший включение информации в реестр</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="createReason" type="oos:zfcs_longTextType">
				<xs:annotation>
					<xs:documentation>Причина для внесения в реестр: WINNER_DEVIATION - уклонение победителя от заключения контракта
ONE_WINNER_DEVIATION - уклонение единственного участника от заключения контракта
PARTICIPANT_DEVIATION_IF_WINNER_DEVIATION - уклонение участника размещения заказа от заключения контракта в случае, когда победитель уклонился от заключения контракта
CANCEL_CONTRACT - расторжение контракта</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="approveReason" type="zfcs_longTextType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Основание для включения в РНП</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="customer" type="zfcs_purchaseOrganizationType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Информация о заказчике, подавшем заявку на включение реестр</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="unfairSupplier">
				<xs:annotation>
					<xs:documentation>Информация о недобросовестном поставщике (подрядчике, исполнителе)</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="fullName" type="oos:zfcs_longTextType">
							<xs:annotation>
								<xs:documentation>Наименование / ФИО недобросовестного поставщика</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="type" type="zfcs_participantTypeEnum">
							<xs:annotation>
								<xs:documentation>Тип :
P - Физическое лицо РФ;
PF - Физическое лицо иностранного государства;
U - Юридическое лицо РФ;
UF - Юридическое лицо иностранного государства.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="firmName" type="zfcs_longTextType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Фирменное наименование </xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="inn" minOccurs="0">
							<xs:annotation>
								<xs:documentation>ИНН (или аналог ИНН для иностранного поставщика)</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="20"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="kpp" type="base:kppType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>КПП</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="place">
							<xs:annotation>
								<xs:documentation>Место нахождения юридического лица</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:choice>
										<xs:element name="kladr">
											<xs:annotation>
												<xs:documentation>Код КЛАДР - если в РФ</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:complexContent>
													<xs:extension base="oos:zfcs_kladrType">
														<xs:sequence>
															<xs:element name="subjectRF" type="zfcs_kladrTextType" minOccurs="0">
																<xs:annotation>
																	<xs:documentation>Субъект РФ</xs:documentation>
																</xs:annotation>
															</xs:element>
															<xs:element name="area" type="zfcs_kladrTextType" minOccurs="0">
																<xs:annotation>
																	<xs:documentation>Район/город</xs:documentation>
																</xs:annotation>
															</xs:element>
															<xs:element name="city" type="zfcs_kladrTextType" minOccurs="0">
																<xs:annotation>
																	<xs:documentation>Населенный пункт</xs:documentation>
																</xs:annotation>
															</xs:element>
															<xs:element name="street" type="zfcs_kladrTextType" minOccurs="0">
																<xs:annotation>
																	<xs:documentation>Улица</xs:documentation>
																</xs:annotation>
															</xs:element>
															<xs:element name="building" type="zfcs_kladrTextType" minOccurs="0">
																<xs:annotation>
																	<xs:documentation>Дом (корпус, строение)</xs:documentation>
																</xs:annotation>
															</xs:element>
															<xs:element name="office" type="zfcs_kladrTextType" minOccurs="0">
																<xs:annotation>
																	<xs:documentation>Офис (квартира)</xs:documentation>
																</xs:annotation>
															</xs:element>
														</xs:sequence>
													</xs:extension>
												</xs:complexContent>
											</xs:complexType>
										</xs:element>
										<xs:element name="country" type="oos:zfcs_countryRef">
											<xs:annotation>
												<xs:documentation>Код страны в ОКСМ - если не в РФ</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:choice>
									<xs:element name="zip">
										<xs:annotation>
											<xs:documentation>Почтовый индекс</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="20"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="place" type="oos:zfcs_longTextType">
										<xs:annotation>
											<xs:documentation>Место</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="email" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Адрес электронной почты</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="256"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="founders" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Учредители, члены коллегиальных исполнительных органов, лица, исполняющие функции единоличного исполнительного органа юридического лица</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="names" type="zfcs_longTextType">
										<xs:annotation>
											<xs:documentation>ФИО учредителей, членов коллегиальных исполнительных органов, лиц, исполняющих
функции единоличного исполнительного органа юридического лица</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="inn">
										<xs:annotation>
											<xs:documentation>ИНН (или аналог ИНН для иностранного поставщика)</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="20"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="purchase" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Информация о проведенных закупках</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="purchaseNumber" type="base:purchaseNumberType">
							<xs:annotation>
								<xs:documentation>Номер закупки/заказа</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="purchaseObjectInfo" type="oos:zfcs_longTextType">
							<xs:annotation>
								<xs:documentation>Наименование объекта закупки/заказа</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="placingWayName">
							<xs:annotation>
								<xs:documentation>Наименование подспособа определения поставщика</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="500"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="protocolDate" type="xs:date" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Дата проведения электронного аукциона, подведения итогов конкурса или итогов проведения запроса котировок, запроса предложений либо дата признания закупки несостоявшейся</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="lotNumber" type="xs:integer">
							<xs:annotation>
								<xs:documentation>Номер лота в извещении</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="document" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Реквизиты подтверждающего документа</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="name" type="zfcs_longTextType" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Наименование документа</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="date" type="xs:date" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Дата документа</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="number" type="zfcs_documentNumberType" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Номер документа</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="notOosPurchase" type="xs:boolean" fixed="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Извещение о проведении закупки не размещено на официальном сайте</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="contract">
				<xs:annotation>
					<xs:documentation>Информация о контракте</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="regNum" type="zfcs_contract.regNumType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Номер реестровой записи</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="productInfo" type="zfcs_longTextType">
							<xs:annotation>
								<xs:documentation>Объект закупки / Предмет контракта (наименование товара, работ, услуг)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="OKPD" type="zfcs_OKPDRef" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Код продукции</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="currency" type="zfcs_currencyRef" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Валюта контракта</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="price" type="base:moneyType">
							<xs:annotation>
								<xs:documentation>Цена контракта</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="cancel" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Расторжение контракта</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="signDate" type="xs:date" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Дата заключения неисполненного или ненадлежащим образом исполненного контракта</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="performanceDate" type="xs:date">
										<xs:annotation>
											<xs:documentation>Срок исполнения контракта</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="base">
										<xs:annotation>
											<xs:documentation>Основание для расторжения контракта</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:sequence>
												<xs:element name="name" type="zfcs_longTextType" minOccurs="0">
													<xs:annotation>
														<xs:documentation>Наименование документа</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="number" type="zfcs_documentNumberType" minOccurs="0">
													<xs:annotation>
														<xs:documentation>Номер документа</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="date" type="xs:date" minOccurs="0">
													<xs:annotation>
														<xs:documentation>Дата документа</xs:documentation>
													</xs:annotation>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
									<xs:element name="cancelDate" type="xs:date">
										<xs:annotation>
											<xs:documentation>Дата расторжения контракта</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="exclude" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Исключение из реестра</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="excludeDate" type="xs:dateTime">
							<xs:annotation>
								<xs:documentation>Дата для исключения </xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="name" type="zfcs_longTextType">
							<xs:annotation>
								<xs:documentation>Краткое описание</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="date" type="xs:date">
							<xs:annotation>
								<xs:documentation>Дата</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="number" type="zfcs_documentNumberType">
							<xs:annotation>
								<xs:documentation>Номер</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="type">
							<xs:annotation>
								<xs:documentation>Тип исключения из реестра:
ORDER - Приказ об исключении из реестра;
EXCLUSION - Решение суда об исключении сведений из РНП;
ROLLBACK - Решение суда об отмене решения ФАС.</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="10"/>
									<xs:enumeration value="ORDER"/>
									<xs:enumeration value="EXCLUSION"/>
									<xs:enumeration value="ROLLBACK"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="schemeVersion" type="base:schemeVersionType" use="required">
			<xs:annotation>
				<xs:documentation>Версия схемы</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<!--Контроль по 99 статье. Основные документы-->
	<xs:complexType name="zfcs_control99PurchasePlanExtractType">
		<xs:annotation>
			<xs:documentation>Информация из плана закупок (РПЗ), направляемая на контроль по 99 статье в орган контроля</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="planNumber" type="zfcs_purchasePlanNumberType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Реестровый номер плана закупок в ЕИС</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="versionNumber" type="base:versionNumberType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Номер версии плана закупок</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="editionNumber" type="base:versionNumberType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Номер редакции в рамках версии плана закупок</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="id" type="xs:long">
				<xs:annotation>
					<xs:documentation>Идентификатор плана закупок в ЕИС</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="documentType" fixed="purchasePlan">
				<xs:annotation>
					<xs:documentation>Тип документа, направляемого на контроль:
contract - Сведения о контракте;
contractProcedure - Сведения об исполнении (расторжении) контракта;
purchasePlan - План закупок;
tenderPlan2017 - План график с 01.01.2017;
notification - Извещение (конкурсная документация);
protocol - Итоговый протокол определения поставщика, подрядчика, исполнителя;
contractProject - Проект контракта без подписей;
contractProjectChange - Доработанный проект контракта на основании размещенного поставщиком протокола разногласий.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="base:control99DocumentTypeEnum"/>
				</xs:simpleType>
			</xs:element>
			<xs:element name="externalId" type="base:externalIdType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Внешний идентификатор документа</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="refPacketUid" type="base:guidType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Идентификатор принятого извне пакета данных</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ASFPacketUid" type="base:guidType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Уникальный идентификатор для пакета, направляемого в АС ФК</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="controlAuthorityInfo" type="zfcs_control99ControlAuthorityInfoType">
				<xs:annotation>
					<xs:documentation>Сведения об органе контроля</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="commonInfo">
				<xs:annotation>
					<xs:documentation>Общая информация плана закупок</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="planYear" type="zfcs_yearType">
							<xs:annotation>
								<xs:documentation>Финансовый год планирования</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="planPeriod">
							<xs:annotation>
								<xs:documentation>Плановый период </xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="firstYear" type="zfcs_yearType">
										<xs:annotation>
											<xs:documentation>Первый плановый год</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="secondYear" type="zfcs_yearType">
										<xs:annotation>
											<xs:documentation>Второй плановый год</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="confirmDate" type="xs:dateTime">
							<xs:annotation>
								<xs:documentation>Дата утверждения плана закупок</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="controlDirectionDate" type="xs:dateTime">
							<xs:annotation>
								<xs:documentation>Дата/время направления на контроль</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="responsibleContactInfo" type="zfcs_contactPersonType">
							<xs:annotation>
								<xs:documentation>Сведения об исполнителе (ответственном за формирование плана закупок)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="confirmContactInfo">
							<xs:annotation>
								<xs:documentation>Сведения о лице, утвердившем план закупок</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="zfcs_contactPersonType">
										<xs:sequence>
											<xs:element name="position" type="zfcs_control99HeadPositionType">
												<xs:annotation>
													<xs:documentation>Должность</xs:documentation>
												</xs:annotation>
											</xs:element>
										</xs:sequence>
									</xs:extension>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="customerInfo">
				<xs:annotation>
					<xs:documentation>Сведения о заказчике</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="zfcs_control99CustomerInfoType">
							<xs:sequence>
								<xs:element name="OKTMOPPO" type="zfcs_OKTMOPPORef" minOccurs="0">
									<xs:annotation>
										<xs:documentation>ОКТМО публично-правового образования (ППО)</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="ownerInfo" type="zfcs_control99OwnerInfoType">
				<xs:annotation>
					<xs:documentation>Сведения об организации-владельце версии плана закупок</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="positions" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Позиции плана закупок. Указание одного из блоков positions и/или specialPurchases обязательно</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="position" type="zfcs_control99PurchasePlanPositionType" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Позиция плана закупок</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="specialPurchases" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Особые закупки. Указание одного из блоков positions и/или specialPurchases обязательно</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
