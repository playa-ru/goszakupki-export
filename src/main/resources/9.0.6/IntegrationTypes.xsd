						<xs:maxLength value="1000"/>
						<xs:minLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="firmName" type="zfcs_longTextMinType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Фирменное наименование организации</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="INN" type="base:innType">
