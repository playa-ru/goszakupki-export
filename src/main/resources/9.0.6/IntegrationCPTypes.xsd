								<xs:sequence>
									<xs:element name="url" type="base:text1000Type">
										<xs:annotation>
											<xs:documentation>Ссылка для перехода на карточку документа</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
