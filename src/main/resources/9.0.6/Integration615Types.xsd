<?xml version="1.0" encoding="UTF-8"?>
<!-- FCS INTEGRATION_TYPES Integration Scheme, version 9.0, create date 25.12.2018 -->
<xs:schema xmlns:base="http://zakupki.gov.ru/oos/base/1" xmlns:cmn="http://zakupki.gov.ru/oos/common/1" xmlns="http://zakupki.gov.ru/oos/pprf615types/1" xmlns:pp615="http://zakupki.gov.ru/oos/pprf615types/1" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://zakupki.gov.ru/oos/pprf615types/1" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:import namespace="http://zakupki.gov.ru/oos/base/1" schemaLocation="BaseTypes.xsd"/>
	<xs:import namespace="http://zakupki.gov.ru/oos/common/1" schemaLocation="CommonTypes.xsd"/>
	<!--Основные документы-->
	<xs:complexType name="notificationPOType">
		<xs:annotation>
			<xs:documentation>Извещение о проведении ПО (предварительный отбор);
 внесение изменений</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="purchaseNotificationType">
				<xs:sequence>
					<xs:element name="purchaseResponsibleInfo">
						<xs:annotation>
							<xs:documentation>Информация об организации, осуществляющей закупку</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="responsibleRole" type="base:responsibleRolePOType">
									<xs:annotation>
										<xs:documentation>Роль организации, осуществляющей закупку:
RPO - Орган, уполномоченный на ведение реестра квалифицированных подрядных организацийии тех-нического заказчика</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="responsibleOrgInfo" type="purchaseOrganizationInfoType">
									<xs:annotation>
										<xs:documentation>Организация, осуществляющая закупку. При приеме содержимое контролируется на  присутствие в справочнике "Сводный перечень заказчиков (СПЗ)" nsiOrganization</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="responsibleInfo" type="contactInfoType">
									<xs:annotation>
										<xs:documentation>Контактная информация организации, осуществляющей закупку</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="specializedOrgInfo" type="purchaseOrganizationInfoType" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Специализированная организация. При приеме содержимое контролируется на  присутствие в справочнике "Сводный перечень заказчиков (СПЗ)" nsiOrganization</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="specializedInfo" type="contactInfoType" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Контактная информация специализированной организации</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="notificationInfo" type="notificationPOInfoType">
						<xs:annotation>
							<xs:documentation>Информация о проведении ПО (предварительный отбор)</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="protocolPOType">
		<xs:annotation>
			<xs:documentation>Протокол предварительного отбора в ПО</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="purchaseProtocolType">
				<xs:sequence>
					<xs:element name="protocolPublisherInfo">
						<xs:annotation>
							<xs:documentation>Информация об организации, разместившей протокол</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="publisherOrgInfo" type="purchaseOrganizationInfoType">
									<xs:annotation>
										<xs:documentation>Организация, разместившая протокол. При приеме содержимое контролируется на присутствие в справочнике "Сводный перечень заказчиков (СПЗ)" nsiOrganization</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="publisherRole" type="base:responsibleRolePOType">
									<xs:annotation>
										<xs:documentation>Роль организации, осуществляющей закупку:
RPO - Орган, уполномоченный на ведение реестра квалифицированных подрядных организацийии технического заказчика</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="protocolInfo" type="protocolPOInfoType">
						<xs:annotation>
							<xs:documentation>Информация о проведении предварительного отбора</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="qualifiedContractorType">
		<xs:annotation>
			<xs:documentation>Сведения о квалифицированной подрядной организации для включения в РКПО по ПП РФ № 615</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="id" type="xs:long" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Идентификатор документа ЕИС</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="externalId" type="base:externalIdType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Внешний идентификатор документа</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="versionNumber" type="base:versionNumberType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Номер версии</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="commonInfo">
				<xs:annotation>
					<xs:documentation>Общая информация</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="registryCommonInfoType">
							<xs:choice>
								<xs:element name="publishedInEISPOInfo">
									<xs:annotation>
										<xs:documentation>Сведения о предварительном отборе размещены в ЕИС</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:sequence>
											<xs:element name="purchaseNumber" type="base:purchaseNumber615Type">
												<xs:annotation>
													<xs:documentation>Реестровый номер предварительного отбора</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="protocolPOProperties" minOccurs="0">
												<xs:annotation>
													<xs:documentation>Реквизиты протокола предварительного отбора. Игнорируется при приёме, заполняется заполняется из протокола предвапрительного отбора (protocolPOType). Заполняется при передаче </xs:documentation>
												</xs:annotation>
												<xs:simpleType>
													<xs:restriction base="xs:string">
														<xs:minLength value="1"/>
														<xs:maxLength value="500"/>
													</xs:restriction>
												</xs:simpleType>
											</xs:element>
											<xs:element name="decisionDate" type="xs:date" minOccurs="0">
												<xs:annotation>
													<xs:documentation>Дата принятия решения о включении участника предварительного отбора в реестр квалифицированных подрядных организаций. Игнорируется при приёме, заполняется из протокола предвапрительного отбора (protocolPOType) зачением  "Дата размещения документа" commonInfo/docPublishDate Заполняется при передаче</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="registryInfo" minOccurs="0">
												<xs:annotation>
													<xs:documentation>Информация о реестровой записи. Игнорируется при приёме, заполняется при передаче</xs:documentation>
												</xs:annotation>
												<xs:complexType>
													<xs:sequence>
														<xs:element name="keepingRegisterOrg" type="base:organizationRef" minOccurs="0">
															<xs:annotation>
																<xs:documentation>Орган по ведению реестра квалифицированных подрядных организаций. Игнорируется при приёме, заполняется из извещения предвапрительного отбора (notificationPOType). Заполняется при передаче</xs:documentation>
															</xs:annotation>
														</xs:element>
														<xs:element name="region" type="kladrType" minOccurs="0">
															<xs:annotation>
																<xs:documentation>Регион организации в классификации КЛАДР. Игнорируется при приёме, заполняется из извещения предвапрительного отбора (notificationPOType). Заполняется при передаче</xs:documentation>
															</xs:annotation>
														</xs:element>
														<xs:element name="validity" minOccurs="0">
															<xs:annotation>
																<xs:documentation>Период, на который подрядная организация включается в реестр квалифицированных подрядных организаций. Игнорируется при приёме, заполняется при передаче </xs:documentation>
															</xs:annotation>
															<xs:complexType>
																<xs:sequence>
																	<xs:element name="startDate" type="xs:date" minOccurs="0">
																		<xs:annotation>
																			<xs:documentation>Дата внесения информации в реестр квалифицированных подрядных организаций. Игнорируется при приёме, заполняется при передаче </xs:documentation>
																		</xs:annotation>
																	</xs:element>
																	<xs:element name="endDate" type="xs:date" minOccurs="0">
																		<xs:annotation>
																			<xs:documentation>Дата окончания периода, на который подрядная организация включается в реестр квалифицированных подрядных организаций. Игнорируется при приёме, заполняется при передаче </xs:documentation>
																		</xs:annotation>
																	</xs:element>
																</xs:sequence>
															</xs:complexType>
														</xs:element>
													</xs:sequence>
												</xs:complexType>
											</xs:element>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="notPublishedInEISPOInfo">
									<xs:annotation>
										<xs:documentation>Сведения о предварительном отборе не размещены в ЕИС</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:sequence>
											<xs:element name="purchaseNumber">
												<xs:annotation>
													<xs:documentation>Реестровый номер предварительного отбора</xs:documentation>
												</xs:annotation>
												<xs:simpleType>
													<xs:restriction base="xs:string">
														<xs:minLength value="1"/>
														<xs:maxLength value="50"/>
													</xs:restriction>
												</xs:simpleType>
											</xs:element>
											<xs:element name="protocolPOProperties" minOccurs="0">
												<xs:annotation>
													<xs:documentation>Реквизиты протокола предварительного отбора</xs:documentation>
												</xs:annotation>
												<xs:simpleType>
													<xs:restriction base="xs:string">
														<xs:minLength value="1"/>
														<xs:maxLength value="500"/>
													</xs:restriction>
												</xs:simpleType>
											</xs:element>
											<xs:element name="decisionDate" type="xs:date">
												<xs:annotation>
													<xs:documentation>Дата принятия решения о включении участника предварительного отбора в реестр квалифицированных подрядных организаций</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="registryInfo">
												<xs:annotation>
													<xs:documentation>Информация о реестровой записи</xs:documentation>
												</xs:annotation>
												<xs:complexType>
													<xs:sequence>
														<xs:element name="keepingRegisterOrg" type="base:organizationRef">
															<xs:annotation>
																<xs:documentation>Орган по ведению реестра квалифицированных подрядных организаций. Игнорируется при приёме, заполняется при передаче</xs:documentation>
															</xs:annotation>
														</xs:element>
														<xs:element name="region" type="kladrType">
															<xs:annotation>
																<xs:documentation>Регион организации в классификации КЛАДР. При приёме проверяется наличие значение в КЛАДР</xs:documentation>
															</xs:annotation>
														</xs:element>
														<xs:element name="validity">
															<xs:annotation>
																<xs:documentation>Период, на который подрядная организация включается в реестр квалифицированных подрядных организаций</xs:documentation>
															</xs:annotation>
															<xs:complexType>
																<xs:sequence>
																	<xs:element name="startDate" type="xs:date">
																		<xs:annotation>
																			<xs:documentation>Дата внесения информации в реестр квалифицированных подрядных организаций</xs:documentation>
																		</xs:annotation>
																	</xs:element>
																	<xs:element name="endDate" type="xs:date" minOccurs="0">
																		<xs:annotation>
																			<xs:documentation>Дата окончания периода, на который подрядная организация включается в реестр квалифицированных подрядных организаций. Игнорируется при приёме, заполняется при передаче </xs:documentation>
																		</xs:annotation>
																	</xs:element>
																</xs:sequence>
															</xs:complexType>
														</xs:element>
													</xs:sequence>
												</xs:complexType>
											</xs:element>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
							</xs:choice>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="contractorInfo">
				<xs:annotation>
					<xs:documentation>Сведения о подрядной организации</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:choice>
						<xs:element name="legalEntityRFInfo">
							<xs:annotation>
								<xs:documentation>Юридическое лицо РФ</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="legalEntityRFType">
										<xs:sequence>
											<xs:element name="legalForm" type="base:OKOPFRef" minOccurs="0">
												<xs:annotation>
													<xs:documentation>Организационно-правовая форма организации в ОКОПФ. При приеме содержимое контролируется на  присутствие в справочнике "Общероссийский классификатор организационно-правовых форм (ОКОПФ)" (nsiOKOPF)</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="shortName" type="base:text2000Type" minOccurs="0">
												<xs:annotation>
													<xs:documentation>Сокращенное наименование организации</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="firmName" type="base:text2000Type" minOccurs="0">
												<xs:annotation>
													<xs:documentation>Фирменное наименование организации</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="registrationDate" type="xs:date" minOccurs="0">
												<xs:annotation>
													<xs:documentation>Дата поставновки на учет в налоговом органе</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="personWithRight" type="personType">
												<xs:annotation>
													<xs:documentation>Лицо, имеющее право действовать без доверенности от имени юридического лица</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="contactPerson" type="personType" minOccurs="0">
												<xs:annotation>
													<xs:documentation>Контактное лицо</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="eMail" type="base:eMailType">
												<xs:annotation>
													<xs:documentation>Адрес электронной почты</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="phone" type="base:phoneType">
												<xs:annotation>
													<xs:documentation>Номер телефона организации</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="phones" minOccurs="0">
												<xs:annotation>
													<xs:documentation>Дополнительные номера телефонов организации</xs:documentation>
												</xs:annotation>
												<xs:complexType>
													<xs:sequence>
														<xs:element name="phone" type="base:phoneType" maxOccurs="unbounded">
															<xs:annotation>
																<xs:documentation>Номер телефона организации</xs:documentation>
															</xs:annotation>
														</xs:element>
													</xs:sequence>
												</xs:complexType>
											</xs:element>
										</xs:sequence>
									</xs:extension>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
						<xs:element name="legalEntityForeignStateInfo">
							<xs:annotation>
								<xs:documentation>Юридическое лицо иностранного государства</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="legalEntityForeignStateType">
										<xs:sequence>
											<xs:element name="shortName" type="base:text2000Type" minOccurs="0">
												<xs:annotation>
													<xs:documentation>Сокращенное наименование организации</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="firmName" type="base:text2000Type" minOccurs="0">
												<xs:annotation>
													<xs:documentation>Фирменное наименование организации</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="registrationDate" type="xs:date" minOccurs="0">
												<xs:annotation>
													<xs:documentation>Дата поставновки на учет в налоговом органе</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="personWithRight" type="personType">
												<xs:annotation>
													<xs:documentation>Лицо, имеющее право действовать без доверенности от имени юридического лица</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="contactPerson" type="personType" minOccurs="0">
												<xs:annotation>
													<xs:documentation>Контактное лицо</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="eMail" type="base:eMailType">
												<xs:annotation>
													<xs:documentation>Адрес электронной почты</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="phone" type="base:phoneType">
												<xs:annotation>
													<xs:documentation>Номер телефона организации</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="phones" minOccurs="0">
												<xs:annotation>
													<xs:documentation>Дополнительные номера телефонов организации</xs:documentation>
												</xs:annotation>
												<xs:complexType>
													<xs:sequence>
														<xs:element name="phone" type="base:phoneType" maxOccurs="unbounded">
															<xs:annotation>
																<xs:documentation>Номер телефона организации</xs:documentation>
															</xs:annotation>
														</xs:element>
													</xs:sequence>
												</xs:complexType>
											</xs:element>
										</xs:sequence>
									</xs:extension>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
						<xs:element name="individualPersonInfo">
							<xs:annotation>
								<xs:documentation>Индивидуальный предприниматель</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="individualPersonType">
										<xs:sequence>
											<xs:element name="registrationDate" type="xs:date" minOccurs="0">
												<xs:annotation>
													<xs:documentation>Дата поставновки на учет в налоговом органе</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="eMail" type="base:eMailType">
												<xs:annotation>
													<xs:documentation>Адрес электронной почты</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="phone" type="base:phoneType">
												<xs:annotation>
													<xs:documentation>Номер телефона организации</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="phones" minOccurs="0">
												<xs:annotation>
													<xs:documentation>Дополнительные номера телефонов организации</xs:documentation>
												</xs:annotation>
												<xs:complexType>
													<xs:sequence>
														<xs:element name="phone" type="base:phoneType" maxOccurs="unbounded">
															<xs:annotation>
																<xs:documentation>Номер телефона организации</xs:documentation>
															</xs:annotation>
														</xs:element>
													</xs:sequence>
												</xs:complexType>
											</xs:element>
										</xs:sequence>
									</xs:extension>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
					</xs:choice>
